#Circonscriptions administratives

#http://data.ign.fr/id/cadastrenap/departement/DEPT_Seine_et_Oise
nap:DEPT_Seine_et_Oise a nap:Landmark;
    nap:isLandmarkType nap:Departement;
    rdfs:label "Seine-et-Oise"@fr.

#http://data.ign.fr/id/cadastrenap/arrondissement/ARR_Corbeil
nap:ARR_Corbeil a nap:Landmark;
    nap:isLandmarkType nap:Arrondissement;
    rdfs:label "Arrondissement de Corbeil"@fr;
    nap:hasSpatialDescription [
        a nap:SpatialDescription;
        nap:targets nap:ARR_Corbeil;
        nap:firstStep [
            a nap:Segment;
            nap:isSpatialRelationType nap:within;
            nap:locatum nap:ARR_Corbeil;
            nap:relatum nap:DEPT_Seine_et_Oise;
        ]].


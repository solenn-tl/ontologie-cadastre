#Circonscriptions administratives

#http://data.ign.fr/id/cadastrenap/departement/DEPT_Seine_et_Oise
nap:DEPT_Seine_et_Oise a nap:Landmark;
    nap:isLandmarkType nap:Departement;
    rdfs:label "Seine-et-Oise"@fr.

#http://data.ign.fr/id/cadastrenap/arrondissement/ARR_Corbeil
nap:ARR_Corbeil a nap:Landmark;
    nap:isLandmarkType nap:Arrondissement;
    rdfs:label "Arrondissement de Corbeil"@fr;
    nap:hasAttribute [
        a nap:SpatialDescription;
        nap:hasVersion [
                nap:targets nap:ARR_Corbeil;
                nap:firstStep [
                    a nap:Segment;
                    nap:isSpatialRelationType nap:within;
                    nap:locatum nap:ARR_Corbeil;
                    nap:relatum nap:DEPT_Seine_et_Oise
                ]]
        ].
    
#http://data.ign.fr/id/cadastrenap/canton/CA_Boissy_Saint_Leger
nap:CA_Boissy_Saint_Leger a nap:Landmark;
    nap:isLandmarkType nap:Canton;
    rdfs:label "Canton de Boissy-Saint-Léger"@fr;
    nap:hasAttribute [
        a nap:SpatialDescription;
        nap:hasVersion [
        nap:targets nap:CA_Boissy_Saint_Leger;
        nap:firstStep [
            a nap:Segment;
            nap:isSpatialRelationType nap:within;
            nap:locatum nap:CA_Boissy_Saint_Leger;
            nap:relatum nap:ARR_Corbeil;
        ]]].

#http://data.ign.fr/id/cadastrenap/commune/COMM_Marolles_en_Brie
nap:COMM_Marolles_en_Brie a nap:Landmark;
    nap:isLandmarkType nap:Commune;
    rdfs:label "Marolles-en-Brie"@fr;
    nap:hasAttribute [
        a nap:SpatialDescription;
        nap:hasVersion [
            nap:targets nap:COMM_Boissy_Saint_Leger;
            nap:firstStep [
                a nap:Segment;
                nap:isSpatialRelationType nap:within;
                nap:locatum nap:COMM_Marolles_en_Brie;
                nap:relatum nap:CA_Boissy_Saint_Leger;
        ]]].

#http://data.ign.fr/id/cadastrenap/section/SEC_Marolles_en_Brie_A
nap:SEC_Marolles_en_Brie_C a nap:Landmark;
    nap:isLandmarkType nap:Section;
    rdfs:label "Section C dite du Village";
    dcterms:identifier "C";
    nap:hasAttribute [
        nap:hasVersion [
        a nap:SpatialDescription;
        nap:targets nap:SEC_Marolles_en_Brie_C;
        nap:firstStep [
            a nap:Segment;
            nap:isSpatialRelationType nap:within;
            nap:locatum nap:SEC_Marolles_en_Brie_C;
            nap:relatum nap:COMM_Marolles_en_Brie;
        ]]].

nap:owner0001 a nap:Owner;
    skos:prefLabel "Mazarot Pierre";
    nap:name "Mazarot Pierre";
    nap:activity "cabaretier";
    nap:address "à Marolles".

#http://data.ign.fr/id/cadastrenap/parcel/Marolles_en_Brie_C_191  
nap:Marolles_en_Brie_C_191 a nap:Landmark;
    nap:isLandmarkType nap:Parcel;
    rdfs:label "Parcelle 191, Section C, Marolles-en-Brie";
    skos:prefLabel "Parcelle 191, Section C, Marolles-en-Brie";
    nap:hasAttribute [ a nap:Owner;
        nap:hasVersion [
            nap:isOwnedBy nap:owner0001;
            nap:hasBeginning "1810";
            nap:hasEnd "1822"#;
            #nap:source nap:source0001;
            #nap:source nap:source0002.
        ]
    ];
    nap:hasAttribute [ a nap:Nature;
        nap:hasVersion [
            nap:isParcelNature nap:Ground;
            nap:hasBeginning "1810";
            nap:hasEnd "entre 1832 et 1882";
            nap:before _:b1
        ]
    ];
    nap:hasAttribute _:b1.
    _:b1 a nap:Nature;
        nap:hasVersion [
            nap:isParcelNature nap:House;
            nap:hasBeginning "entre 1832 et 1882"
        ]
    ;
    nap:hasAttribute[ a nap:SpatialDescription;
        nap:hasVersion [
            rdfs:label "Marolles-en-Brie, Section C, Le Village, Parcelle 191"@fr;
            nap:targets nap:Marolles_en_Brie_C_191;
            nap:firstStep [
                a nap:Segment;
                nap:isSpatialRelationType nap:within;
                nap:locatum nap:Marolles_en_Brie_C_191;
                nap:relatum nap:COMM_Marolles_en_Brie;
                nap:nextStep [
                    a nap:Segment;
                    nap:isSpatialRelationType nap:within;
                    nap:locatum nap:Marolles_en_Brie_C_191;
                    nap:relatum nap:SEC_Marolles_en_Brie_C;
                ]
            ]
        ]
    ].
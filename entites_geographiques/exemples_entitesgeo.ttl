#http://data.ign.fr/id/cadastrenap/owner/owner0001
nap:owner0001 a nap:Owner;
    rdfs:label "Charlier"@fr;#ETS
    skos:altLabel "Charlier Charles antoine"@fr;#MAT_1812 m1
    nap:activity "B<sup>eois</sup>"@fr;#MAT_1812 m1
    nap:address "à Paris"@fr.#MAT_1812 m1

#http://data.ign.fr/id/cadastrenap/parcel/Boissy_Saint_Leger_A_207    
nap:Boissy_Saint_Leger_A_207 a nap:Landmark;
    rdfs:label "207";
    nap:isLandmarkType nap:Parcel;
    nap:isOwnedBy nap:owner0001;
    nap:isParcelNature nap:Ground;
    nap:hasSpatialDescription [
        a nap:SpatialDescription;
        rdfs:label "Boissy-Saint-Léger, Section A, Le Village, Parcelle 207"@fr;
        nap:targets nap:Boissy_Saint_Leger_A_207;
        nap:firstStep [
            a nap:Segment;
            nap:isSpatialRelationType nap:within;
            nap:locatum nap:Boissy_Saint_Leger_A_207;
            nap:relatum nap:COMM_Boissy_Saint_Leger;
            nap:nextStep [
                a nap:Segment;
                nap:isSpatialRelationType nap:within;
                nap:locatum nap:Boissy_Saint_Leger_A_207;
                nap:relatum nap:SEC_Boissy_Saint_Leger_A;
                nap:nextStep [
                    a nap:FinalSegment;
                    nap:isSpatialRelationType nap:within;
                    nap:locatum nap:Boissy_Saint_Leger_A_207;
                    nap:relatum nap:Place0001
                ]
            ]
        ]
    ].


#http://data.ign.fr/id/cadastrenap/parcel/Boissy_Saint_Leger_A_207    
nap:Boissy_Saint_Leger_A_207 a nap:Landmark;
    nap:isLandmarkType nap:Parcel;
    rdfs:label "Parcelle 207, Section A, Boissy-Saint-Léger";
    skos:prefLabel "Parcelle 207, Section A, Boissy-Saint-Léger";
    nap:hasAttribute [ a nap:Owner;
        nap:hasVersion [
            nap:isOwnedBy nap:owner0001;#Est-ce qu'on a le droit inclure une classe dans elle même ?
            time:hasBeginning "1810";
            time:hasEnd "1822";
            nap:sourcedFrom [ 
                nap:hasSource nap:source0001
                nap:hasSource nap:source0002
            ]
        ]
    ];
    nap:hasAttribute [ a nap:Nature;
        nap:hasValue [
            nap:isParcelNature nap:Ground;
            nap:startDate 
            nap:endDate
            nap:sourcedFrom
        ]
    ];
    nap:hasAttribute[ a nap:SpatialDescription;
        nap:hasvalue [
            rdfs:label "Boissy-Saint-Léger, Section A, Le Village, Parcelle 207"@fr;
            nap:targets nap:Boissy_Saint_Leger_A_207;
            nap:firstStep [
                a nap:Segment;
                nap:isSpatialRelationType nap:within;
                nap:locatum nap:Boissy_Saint_Leger_A_207;
                nap:relatum nap:COMM_Boissy_Saint_Leger;
                nap:nextStep [
                    a nap:Segment;
                    nap:isSpatialRelationType nap:within;
                    nap:locatum nap:Boissy_Saint_Leger_A_207;
                    nap:relatum nap:SEC_Boissy_Saint_Leger_A;
                    nap:nextStep [
                        a nap:FinalSegment;
                        nap:isSpatialRelationType nap:within;
                        nap:locatum nap:Boissy_Saint_Leger_A_207;
                        nap:relatum nap:Place0001
                    ]
                ]
            ]
        ]
    ].
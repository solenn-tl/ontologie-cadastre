# ---- Imported annotation properties ----
skos:inScheme a owl:AnnotationProperty.
skos:prefLabel a owl:AnnotationProperty .
skos:altLabel a owl:AnnotationProperty.
skos:broader a owl:AnnotationProperty.
skos:definition a owl:AnnotationProperty.

###############################
# ---- Classes definition ----
###############################

###  http://www.w3.org/2004/02/skos/core#Concept
skos:Concept a owl:Class .

###  http://www.w3.org/2004/02/skos/core#ConceptScheme
skos:ConceptScheme a owl:Class .

###  http://data.ign.fr/def/cadastrenap#SpatialDescription
nap:SpatialDescription a owl:Class ;
             rdfs:subClassOf [ a owl:Restriction ;
                               owl:onProperty nap:firstStep ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass nap:Segment
                             ] ,
                             [ a owl:Restriction ;
                               owl:onProperty nap:targets ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass nap:Landmark
                             ] ;
             rdfs:label "Parcelle localisation"@en , "Localisation d'une parcelle"@fr;
             rdfs:comment "Localisation de la parcelle au sein du découpage cadastral et administratif"@fr.
            #owl:equivalentClass addr:Address.


###  http://data.ign.fr/def/cadastrenap#Segment
nap:Segment a owl:Class ;
                    rdfs:subClassOf [ a owl:Restriction ;
                                      owl:onProperty nap:relatum ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass nap:Landmark
                                    ] ,
                                    [ a owl:Restriction ;
                                      owl:onProperty nap:locatum ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass nap:Landmark
                                    ] ,
                                    [ a owl:Restriction ;
                                      owl:onProperty nap:nextStep ;
                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass nap:Segment
                                    ] ;
                    rdfs:label "Localisation segment"@en ,
                               "Segment de localisation d'une parcelle"@fr.
                    #owl:equivalentClass addr:AddressSegment.


###  http://data.ign.fr/def/cadastrenap#SegmentType
nap:SegmentType a owl:Class ;
                        rdfs:subClassOf skos:Concept ;
                        rdfs:label "Type de segment"@fr ,
                                   "Type of address"@en.
                        #owl:equivalentClass addr:AddressSegmentType.


###  http://data.ign.fr/def/cadastrenap#FinalSegment
nap:FinalSegment a owl:Class ;
                         rdfs:subClassOf nap:Segment ,
                                         [ a owl:Restriction ;
                                           owl:onProperty nap:nextStep ;
                                           owl:cardinality "0"^^xsd:nonNegativeInteger
                                         ] ;
                         rdfs:label "Final segment"@en ,
                                    "Segment final"@fr.
                        #owl:equivalentClass addr:FinalAddressSegment.


###  http://data.ign.fr/def/cadastrenap#Landmark
nap:Landmark a owl:Class ;
              rdfs:label "Repère"@fr.
              #owl:equivalentClass addr:Landmark.

###  http://data.ign.fr/def/cadastrenap#LandmarkType
nap:LandmarkType a owl:Class ;
                  rdfs:subClassOf skos:Concept ;
                  rdfs:label "Type de repère"@fr ,
                             "Type of landmark"@en.
                  #owl:equivalentClass addr:LandmarkType.

###  http://data.ign.fr/def/cadastrenap#ParcelNature
nap:ParcelNature a owl:Class;
  rdfs:label "parcel nature"@en ,
            "nature de parcelle"@fr ;
  rdfs:comment "Nature d'une parcelle."@fr.

#################################################################
#    Object Properties
#################################################################

###  http://data.ign.fr/def/cadastrenap#isLandmarkType
nap:isLandmarkType a owl:ObjectProperty ;
                    rdfs:domain nap:Landmark ;
                    rdfs:range nap:LandmarkType ;
                    rdfs:range nap:LandmarkType ;
                    rdfs:label "le type de repère est"@fr ,
                               "is landmark type"@en .


###  http://data.ign.fr/def/cadastrenap#isParcelNature
nap:isParcelNature a owl:ObjectProperty ;
                    rdfs:domain nap:Parcel ;
                    rdfs:range nap:ParcelNature ;
                    rdfs:label "la nature de la parcelle est"@fr ,
                               "parcel nature is"@en .    

nap:isOwnedBy a owl:ObjectProperty ;
                    rdfs:domain nap:Parcel ;
                    rdfs:range nap:Owner ;
                    rdfs:label "le propriétaire de la parcelle est"@fr ,
                               "parcel owner is"@en ;
                    owl:inverseOf nap:owns.

nap:owns a owl:ObjectProperty ;
                    rdfs:domain nap:Owner ;
                    rdfs:range nap:Parcel ;
                    rdfs:label "possède"@fr ,
                               "owns"@en ;
                    owl:inverseOf nap:isOwnedBy.

###  http://data.ign.fr/def/cadastrenap#hasLocalisation
nap:hasSpatialDescription a owl:ObjectProperty ;
                    rdfs:domain nap:Parcel ;
                    rdfs:range nap:SpatialDescription ;
                    rdfs:label "la localisation de la parcelle est"@fr ,
                               "parcel localisation is"@en .         

###  http://data.ign.fr/def/cadastrenap#targets
nap:targets a owl:ObjectProperty ;
             rdfs:domain nap:SpatialDescription ;
             rdfs:range nap:Landmark ;
             rdfs:comment "Cible le repère décrivant la parcelle."@fr ;
             rdfs:label "cible"@fr ,
                        "targets"@en .

###  http://data.ign.fr/def/cadastrenap#locatum
nap:locatum a owl:ObjectProperty ;
             rdfs:domain nap:Segment ;
             rdfs:range nap:Landmark ;
             rdfs:comment "Donne le repère qui est le locatum pour la relation spatiale"@fr ;
             rdfs:label "a pour locatum"@fr ,
                        "has locatum"@en .

###  http://data.ign.fr/def/cadastrenap#relatum
nap:relatum a owl:ObjectProperty ;
             rdfs:domain nap:Segment ;
             rdfs:range nap:Landmark ;
             rdfs:comment "Donne le repère qui est un des relatums pour la relation spatiale"@fr ;
             rdfs:label "a pour relatum"@fr ,
                        "has relatum"@en .

###  http://data.ign.fr/def/cadastrenap#firstStep
nap:firstStep a owl:ObjectProperty ;
               rdfs:domain nap:SpatialDescription ;
               rdfs:range nap:Segment ;
               rdfs:comment "Première étape de la localisation d'une parcelle."@fr ;
               rdfs:label "premier segment"@fr ,
                          "first step"@en .

###  http://data.ign.fr/def/cadastrenap#nextStep
nap:nextStep a owl:ObjectProperty ;
              rdfs:domain nap:Segment ;
              rdfs:range nap:Segment ;
              rdfs:comment "Etape de localisation d'une parcelle."@fr ;
              rdfs:label "segment suivant"@fr ,
                         "next step"@en.

###  http://data.ign.fr/def/cadastrenap#isSpatialRelationType
nap:isSpatialRelationType a owl:ObjectProperty ;
                           rdfs:domain nap:Segment ;
                           rdfs:range nap:SegmentType ;
                           rdfs:label "est une relation spatiale de type"@fr ,
                                      "is spatial relation type"@en .

###  http://data.ign.fr/def/cadastrenap#within
nap:within a owl:ObjectProperty ;
            rdfs:domain nap:Landmark ;
            rdfs:range nap:Landmark ;
            rdfs:comment "Relation spatiale définissant que le locatum est dans un relatum"@fr ;
            rdfs:label "dans"@fr ,
                       "within"@en.

###  http://data.ign.fr/def/cadastrenap#undefinedSR
nap:undefinedSpatialRelation a owl:ObjectProperty ;
                 rdfs:domain nap:Landmark ;
                 rdfs:range nap:Landmark ;
                 rdfs:comment "Relation spatiale indéfinie entre deux repères"@fr ;
                 rdfs:label "indéfini"@fr ,
                            "undefined"@en.

#################################################################
#    Datatype Properties
#################################################################

nap:nature a owl:DatatypeProperty;
  rdfs:domain nap:Landmark ;
  rdfs:range xsd:string;
  rdfs:label "a pour nature";
  rdfs:comment "DEscription de la nature de la parcelle".

#################################################################
#    Individuals
#################################################################

###  http://data.ign.fr/def/cadastrenap#LandmarkList
nap:LandmarkList a owl:NamedIndividual ,
                           skos:ConceptScheme ;
                  rdfs:comment "Liste des différents rôles pouvant être joués par un repère, représentés comme une liste de concepts SKOS."@fr ;
                  rdfs:label "type de repère"@fr ,
                             "type of landmark"@en .

###  http://data.ign.fr/def/cadastrenap#AdministrativeUnit
nap:AdministrativeUnit a owl:NamedIndividual ,
                                  nap:LandmarkType ,
                                  skos:Concept ;
                         skos:definition "Circonscription administrative (commune, canton, arrondissement, département)"@fr ;
                         skos:inScheme addr:LandmarkList ;
                         skos:prefLabel "circonscription administrative"@fr;
                         skos:altLabel "administrative unit"@en.

###  http://data.ign.fr/def/cadastrenap#Parcel
nap:Parcel a owl:NamedIndividual ,
                   nap:LandmarkType ,
                   skos:Concept ;
          skos:definition "Parcelle cadastrale"@fr ;
          skos:inScheme nap:LandmarkList ;
          skos:prefLabel "parcelle"@fr;
          skos:altLabel "parcel"@en ;
        skos:broader nap:Landmark.

###  http://data.ign.fr/def/cadastrenap#Parcel
nap:Section a owl:NamedIndividual ,
                   nap:LandmarkType ,
                   skos:Concept ;
          skos:definition "Section cadastrale"@fr ;
          skos:inScheme nap:LandmarkList ;
          skos:prefLabel "section"@en ,
                         "section"@fr;
        skos:broader nap:Landmark.

###  http://data.ign.fr/def/cadastrenap#Place
nap:Place a owl:NamedIndividual ,
                   nap:LandmarkType ,
                   skos:Concept ;
          skos:definition "Lieu-dit, canton communal, triage, ou voie utilisé comme repère pour localiser une parcelle dans une section."@fr ;
          skos:inScheme nap:LandmarkList ;
          skos:prefLabel "place"@en ,
                         "lieu-dit"@fr;
        skos:broader nap:Landmark.

###  http://data.ign.fr/def/cadastrenap#Commune
nap:Commune a owl:NamedIndividual ,
                   nap:LandmarkType ,
                   skos:Concept ;
          skos:definition "Entité administrative de type commune"@fr ;
          skos:inScheme nap:LandmarkList ;
          skos:prefLabel "commune"@fr;
          skos:altLabel "town"@en.
        skos:broader nap:AdministrativeUnit.

###  http://data.ign.fr/def/cadastrenap#Canton
nap:Canton a owl:NamedIndividual ,
                   nap:LandmarkType ,
                   skos:Concept ;
          skos:definition "Entité administrative de type canton (tels qu'ils sont décrits aavant la loi de 2013)"@fr ;
          skos:inScheme nap:LandmarkList ;
          skos:prefLabel "canton"@fr,
                         "canton"@en;
          skos:altLabel "justice de paix"@fr;
        skos:broader nap:AdministrativeUnit.

###  http://data.ign.fr/def/cadastrenap#Arrondissement
nap:Arrondissement a owl:NamedIndividual ,
                   nap:LandmarkType ,
                   skos:Concept ;
          skos:definition "Entité administrative de type arrondissement"@fr ;
          skos:inScheme nap:LandmarkList ;
          skos:prefLabel "arrondissement"@fr,
                        "arrondissement"@en;
        skos:broader nap:AdministrativeUnit.

###  http://data.ign.fr/def/cadastrenap#Departement
nap:Departement a owl:NamedIndividual ,
                   nap:LandmarkType ,
                   skos:Concept ;
          skos:definition "Entité administrative de type département"@fr ;
          skos:inScheme nap:LandmarkList ;
          skos:prefLabel "département"@fr,
                        "département"@en;
          skos:broader nap:AdministrativeUnit.

###  http://data.ign.fr/def/cadastrenap#SegmentList
nap:SegmentList a owl:NamedIndividual ,
                        skos:ConceptScheme ;
                        rdfs:comment "Liste des différents types de relations spatiales pouvant être associées à un segment de localisation, représentés comme une liste de concepts SKOS."@fr ;
                        rdfs:label "type de segment"@fr ,
                                   "type of segment"@en .

###  http://data.ign.fr/def/cadastrenap#Within
nap:Within a owl:NamedIndividual ,
                     nap:SegmentType ,
                     skos:Concept ;
            skos:prefLabel "à l'intérieur"@fr ;
            skos:definition "Relation spatiale définissant que le locatum est situé dans le relatum"@fr ;
            skos:inScheme nap:AdressSegmentList ;
            skos:prefLabel "dans"@fr ,
                           "within"@en .

###  http://data.ign.fr/def/cadastrenap#UndifinedSpatialRelation
nap:UndifinedSpatialRelation a owl:NamedIndividual ,
                     nap:SegmentType ,
                     skos:Concept ;
            skos:prefLabel "relation indéfinie"@fr,
                            "undifined relation"@en ;
            skos:definition "Relation spatiale indéfinie entre le locatum et le relatum"@fr ;
            skos:inScheme nap:SegmentList.

###  http://data.ign.fr/def/cadastrenap#SegmentList
nap:ParcelNatureList a owl:NamedIndividual ,
                        skos:ConceptScheme ;
                        rdfs:comment "Liste des différentes natures de parcelles, représentées comme une liste de concepts SKOS."@fr ;
                        rdfs:label "type de parcelle"@fr ,
                                   "type of parcel"@en .




nap:ProprieteNonBatie a owl:NamedIndividual, 
                        nap:ParcelNature, 
                        skos:Concept;
	skos:definition "Propriété non bâtie."@fr;
	skos:inScheme nap:nap:ParcelNatureList;
	skos:prefLabel "propriété non bâtie"@fr;
  skos:boader nap:ParcelNature.

nap:ProprieteBatie a owl:NamedIndividual, 
                        nap:ParcelNature, 
                        skos:Concept;
	skos:definition "Propriété bâtie."@fr;
	skos:inScheme nap:ParcelNatureList;
	skos:prefLabel "propriété bâtie"@fr;
  skos:boader nap:ParcelNature.

nap:BareGround a skos:Concept;
	skos:definition "Terre"@fr, "Bare ground"@en;
	skos:inScheme nap:ParcelNatureList;
	skos:prefLabel "terre"@fr, "bare ground"@en;
  skos:boader nap:ProprieteNonBatie.

nap:Vine a skos:Concept;
	skos:definition "Vigne"@fr, "Vine"@en;
	skos:inScheme nap:ParcelNatureList;
	skos:prefLabel "vigne"@fr, "vine"@en;
  skos:boader nap:ProprieteNonBatie.

nap:VineGround a skos:Concept;
	skos:definition "Terre à vigne"@fr;
	skos:inScheme nap:ParcelNatureList;
	skos:prefLabel "terre à vigne"@fr;
  skos:boader nap:ProprieteNonBatie.

nap:House a skos:Concept;
	skos:definition "Maison"@fr, "House"@fr;
	skos:inScheme nap:ParcelNatureList;
	skos:prefLabel "maison"@fr, "house"@en;
  skos:boader nap:ProprietiBatie.
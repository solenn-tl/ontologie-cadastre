@prefix mlclasse: <http://data.ign.fr/id/codes/cadastrenap/mlClasse/>.

# ---- Imported properties for annotation ----
skos:inScheme rdf:type owl:AnnotationProperty.
skos:prefLabel rdf:type owl:AnnotationProperty .
skos:altLabel rdf:type owl:AnnotationProperty.
skos:broader rdf:type owl:AnnotationProperty.
skos:definition rdf:type owl:AnnotationProperty.

###############################
# ---- Classes definition ----
###############################

# ---- Imported classes ----

# https://www.ica.org/standards/RiC/ontology#RecordSet
rico:RecordSet a owl:Class ;
	rdfs:subClassOf rico:RecordResource ;
	rdfs:comment """One or more records that are grouped together by an Agent based on
         the records sharing one or more attributes or relations."""@en ;
	rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
	rdfs:label "Record Set"@en, "Ensemble d’objets informationnels"@fr, "Agrupación documental"@es ;
	rico:RiCCMCorrespondingComponent """Corresponds to RiC-E03 (Record Set
         entity)"""@en .

# https://www.ica.org/standards/RiC/ontology#Record
rico:Record a owl:Class ;
	rdfs:subClassOf rico:RecordResource ;
	rdfs:comment """Discrete information content formed and inscribed, at least once,
         by any method on any carrier in any persistent, recoverable form by an Agent in the course
         of life or work activity."""@en ;
	rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
	rdfs:label "Record"@en, "Objet informationnel"@fr, "Documento"@es ;
	rico:RiCCMCorrespondingComponent """Corresponds to RiC-E04 (Record
         entity)"""@en .

# https://www.ica.org/standards/RiC/ontology#RecordPart
rico:RecordPart a owl:Class ;
	rdfs:subClassOf rico:RecordResource ;
	rdfs:comment """Component of a Record with independent information content that
         contributes to the intellectual completeness of the Record."""@en ;
	rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
	rdfs:label "Record Part"@en, "Partie d’objet informationnel"@fr, "Componente documental"@es ;
	rico:RiCCMCorrespondingComponent """Corresponds to RiC-E05 (Record Part
         entity)"""@en .

# https://www.ica.org/standards/RiC/ontology#Instantiation
rico:Instantiation
  a owl:Class ;
  #rdfs:subClassOf rico:Thing ;
  rdfs:comment """The inscription of information made by an Agent on a physical
         carrier in any persistent, recoverable form as a means of communicating information through
         time and space."""@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "Instantiation"@en, "Instanciation"@fr, "Instanciación"@es ;
  rico:RiCCMCorrespondingComponent """Corresponds to RiC-E06 (Instantiation
         entity)"""@en ;
  rico:closeTo "PREMIS Representation"@en .

prov:Activity a owl:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "Activity" ;
    owl:disjointWith prov:Entity.

prov:Agent a owl:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "Agent" ;
    owl:disjointWith prov:InstantaneousEvent.

prov:SoftwareAgent a owl:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "SoftwareAgent" ;
    rdfs:subClassOf prov:Agent.

# https://github.com/johnBradley501/FPO/raw/master/fpo.owl#Assertion
fpo:Assertion a owl:Class;
	rdfs:isDefinedBy <https://www.kcl.ac.uk/factoid-prosopography/fpo-factoids#>.

# https://github.com/johnBradley501/FPO/raw/master/fpo.owl#Factoid
fpo:Factoid a owl:Class ;
	rdfs:subClassOf fpo:Assertion ;
	rdfs:isDefinedBy <https://www.kcl.ac.uk/factoid-prosopography/fpo-factoids#>.

#https://github.com/johnBradley501/FPO/raw/master/fpo.owl#Source
fpo:Source a owl:Class ;
	rdfs:isDefinedBy <https://www.kcl.ac.uk/factoid-prosopography/fpo-sources#>.

# https://github.com/johnBradley501/FPO/raw/master/fpo.owl#SourceCitation
fpo:SourceCitation a owl:Class ;
	rdfs:isDefinedBy <https://www.kcl.ac.uk/factoid-prosopography/fpo-sources#>.

# https://data.ign.fr/def/cadastrenap#SourceType
src:SourceType a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Type de source"@fr, "Type of source"@en.

# https://data.ign.fr/def/cadastrenap#MLClasse
src:MLClasse a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Classe d'un document"@fr, "Classe of document"@en.

#####################################
# ---- Objectproperty definition ----
#####################################

# ---- Imported properties ----

rico:includesOrIncluded a owl:ObjectProperty ;
	rdfs:subPropertyOf rico:hasOrHadPart, rico:isRecordResourceAssociatedWithRecordResource ;
	owl:inverseOf rico:isOrWasIncludedIn ;
	rdfs:domain rico:RecordSet ;
	rdfs:range [
		rdf:type owl:Class ;
		owl:unionOf (
			rico:Record
			rico:RecordSet
		) ;
	] ;
	rdfs:comment """Connects a Record Set to a Record or Record Set it aggregates, or
         aggregated in the past."""@en ;
	rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
	rdfs:label "includes or included"@en, "inclut ou a inclus"@fr, "incluye o incluía"@es ;
	rico:RiCCMCorrespondingComponent """RiC-R024 ('includes or included')""".

rico:isOrWasIncludedIn
  a owl:ObjectProperty ;
  rdfs:subPropertyOf rico:isOrWasPartOf, rico:isRecordResourceAssociatedWithRecordResource ;
  owl:inverseOf rico:includesOrIncluded ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (
     rico:Record
     rico:RecordSet
   )
  ] ;
  rdfs:range rico:RecordSet ;
  rdfs:comment "Inverse of 'includes or included' object property."@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "is or was included in"@en, "est ou a été inclus dans"@fr, "está o estaba incluido en"@es ;
  rico:RiCCMCorrespondingComponent """RiC-R024i ('is or was included in'
         relation)"""@en .


rico:hasOrHadComponent a owl:ObjectProperty ;
	rdfs:subPropertyOf rico:hasOrHadPart, rico:isInstantiationAssociatedWithInstantiation ;
	owl:inverseOf rico:isOrWasComponentOf ;
	rdfs:domain rico:Instantiation ;
	rdfs:range rico:Instantiation ;
	rdfs:comment """Connects an Instantiation to one of its present or past component
         instantiations."""@en ;
	rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
	rdfs:label "has or had component"@en, "a ou a eu pour composant"@fr, "tiene o tenía como componente"@es ;
	rico:RiCCMCorrespondingComponent """RiC-R004 ('has or had component'
         relation)"""@en .

rico:hasOrHadInstantiation a owl:ObjectProperty ;
	rdfs:subPropertyOf rico:isRelatedTo ;
	owl:inverseOf rico:isOrWasInstantiationOf ;
	rdfs:domain rico:RecordResource ;
	rdfs:range rico:Instantiation ;
	owl:propertyChainAxiom (
		rico:thingIsSourceOfRelation
		rico:recordResourceToInstantiationRelation_role
		rico:relationHasTarget
	) ;
	rdfs:comment """Connects a Record Resource to an Instantiation, which either may
         exist or may have been lost or destroyed."""@en ;
	rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
	rdfs:label "has or had instantiation"@en, "a ou a eu pour instanciation"@fr, "tiene o tenía como instanciación"@es ;
	rico:RiCCMCorrespondingComponent """RiC-R025 ('has or had instantiation'
         relation)"""@en .

rico:isOrWasInstantiationOf
  a owl:ObjectProperty ;
  rdfs:subPropertyOf rico:isRelatedTo ;
  owl:inverseOf rico:hasOrHadInstantiation ;
  rdfs:domain rico:Instantiation ;
  rdfs:range rico:RecordResource ;
  owl:propertyChainAxiom (
   rico:thingIsTargetOfRelation
   rico:recordResourceToInstantiationRelation_role
   rico:relationHasSource
 ) ;
  rdfs:comment """Inverse of 'has or had instantiation' object
         property."""@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "is or was instantiation of"@en, "est ou a été une instanciation de"@fr, "es o era instanciación de"@es ;
  rico:RiCCMCorrespondingComponent """RiC-R025i ('is or was instantiation of'
         relation)"""@en .

rico:hasOrHadDigitalInstantiation a owl:ObjectProperty ;
	rdfs:subPropertyOf rico:hasOrHadInstantiation ;
	owl:inverseOf rico:isOrWasDigitalInstantiationOf ;
	rdfs:domain rico:RecordResource ;
	rdfs:range rico:Instantiation ;
	rdfs:comment """Connects a record resource to one of its digital instantiations,
         whether it exists or has been lost or destroyed."""@en ;
	rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
	rdfs:label "has or had digital instantiation"@en, "a ou a eu pour instanciation numérique"@fr, "tiene o tenía como instanciación digital"@es .

prov:wasAssociatedWith a owl:ObjectProperty ;
    rdfs:comment "An prov:Agent that had some (unspecified) responsibility for the occurrence of this prov:Activity."@en ;
    rdfs:domain prov:Activity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "wasAssociatedWith" ;
    rdfs:range prov:Agent ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    owl:propertyChainAxiom (prov:qualifiedAssociation
        prov:agent
    ).

prov:used a owl:ObjectProperty ;
    rdfs:comment "A prov:Entity that was used by this prov:Activity. For example, :baking prov:used :spoon, :egg, :oven ."@en ;
    rdfs:domain prov:Activity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "used" ;
    rdfs:range prov:Entity ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    owl:propertyChainAxiom (prov:qualifiedUsage
        prov:Entity
    ).

prov:wasGeneratedBy a owl:ObjectProperty ;
    rdfs:domain prov:Entity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "wasGeneratedBy" ;
    rdfs:range prov:Activity ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    owl:propertyChainAxiom (prov:qualifiedGeneration
        prov:activity
    ).

#https://github.com/johnBradley501/FPO/raw/master/fpo.owl#fromSource
fpo:fromSource a owl:ObjectProperty ;
	rdfs:domain fpo:SourceCitation ;
	rdfs:range fpo:Source .

#https://github.com/johnBradley501/FPO/raw/master/fpo.owl#sourcedFrom
fpo:sourcedFrom a owl:ObjectProperty ;
	rdfs:domain fpo:Assertion ;
	rdfs:range fpo:SourceCitation .

###  http://data.ign.fr/def/cadastrenap#hasSourceType
src:hasSourceType a owl:ObjectProperty ;
                    rdfs:range src:SourceType ;
                    rdfs:comment "Décrit le type de source."@fr ;
                    rdfs:label "est une source de type"@fr ,
                               "is source type"@en .

###  http://data.ign.fr/def/cadastrenap#hasClasse
src:hasClasse a owl:ObjectProperty ;
                    rdfs:comment "Indique qu'un document ou d'une zone de document possède au moins une classe attribuée manuellement ou automatique."@fr ;
                    rdfs:label "a au moins une classe"@fr ,
                               "has at least one classe"@en .

###  http://data.ign.fr/def/cadastrenap#cad:hasClasseValue
src:hasClasseValue a owl:ObjectProperty ;
                    rdfs:comment "Classe attribuée à un document ou à une zone de document par un prov:Agent."@fr ;
                    rdfs:label "a pour classe"@fr ,
                               "has classe value"@en .

###  http://data.ign.fr/def/cadastrenap#isLocalisedIn
src:isLocalisedIn a owl:ObjectProperty ;
                    rdfs:domain rico:Instantiation;
                    rdfs:range add:Landmark ;
                    rdfs:comment "Indique la localisation d'un document physique."@fr ;
                    rdfs:label "est localisé à"@fr ,
                               "is located in"@en .

###  http://data.ign.fr/def/cadastrenap#hasCreationDate
src:hasCreationDate a owl:ObjectProperty ;
                    rdfs:range add:TemporalEntity ;
                    rdfs:comment "Indique la date de création d'une source."@fr ;
                    rdfs:label "a pour date de création"@fr ,
                               "has creation date"@en .

###  http://data.ign.fr/def/cadastrenap#hasValidTime
src:hasValidTime a owl:ObjectProperty ;
                    rdfs:range add:TemporalInterval ;
                    rdfs:comment "Indique la date de validité/d'usage d'un document, pendant lequel il a été complété/utilisé par un ou plusieurs agents. Correcpond également à la période de validité des informations qu'il contient (ex: dans le cas d'une copie ultérieure d'un ancien document abimé)"@fr ;
                    rdfs:label "a pour temps valide"@fr ,
                               "has valid time"@en .

###############################
# ---- Datatype properties ----
###############################

# ---- Imported properties ----
rdfs:label a owl:DatatypeProperty.

rico:identifier a owl:DatatypeProperty ;
  rdfs:domain rico:Thing ;
  rdfs:range rdfs:Literal ;
  rdfs:comment """A word, number, letter, symbol, or any combination of these used
         to uniquely identify or reference an individual instance of an entity within a specific
         information domain. Can include Global Persistent Identifiers (globally unique and
         persistently resolvable identifier for the entity) and/or Local Identifiers. Both the
         domain within which the identifier is unique, and the rules used in forming the identifier
         value should be provided with the identifier value."""@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "identifier"@en, "identifiant"@fr, "identificador"@es ;
  rico:RiCCMCorrespondingComponent """Corresponds to RiC-A22 (Identifier
         attribute)"""@en .

rico:name a owl:DatatypeProperty ;
  rdfs:domain rico:Thing ;
  rdfs:range rdfs:Literal ;
  rdfs:comment """A label, title or term designating an entity in order to make it
         distinguishable from other similar entities."""@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "name"@en, "nom"@fr, "nombre"@es ;
  rico:RiCCMCorrespondingComponent """Corrresponds to RiC-A28 (Name
         attribute)"""@en .

src:transcription a owl:DatatypeProperty ;
  rdfs:range rdfs:Literal ;
  rdfs:label "transcription"@en, "transcription"@fr;
  rdfs:comment "Transcription d'une zone obtenue automatiquement ou manuellement à partir d'un document numérisé.".

src:coordinatesInImageRef a owl:DatatypeProperty ;
  rdfs:range rdfs:Literal ;
  rdfs:label "coordinates in image"@en, "coordonnées dans l'image"@fr;
  rdfs:comment "Coordonnées (référentiel image) d'une zone annotée (manuellement ou automatiquement) dans un document numérisé.".

src:URL a owl:DatatypeProperty ;
  rdfs:range rdfs:Literal ;
  rdfs:label "url de la source"@fr, "source URL"@en;
  rdfs:comment 'Correspond à l\'URL de la source (image, base de donnée)'.
# ---- Ontology metadata -----
<http://data.ign.fr/def/cadastrenap> a owl:Ontology, voaf:Vocabulary ;
        dcterms:creator [
	      rdf:type foaf:Person ;
	      organization:hasMembership [
	          rdf:type organization:Membership ;
	          organization:memberDuring [
	              rdf:type time:Interval ;
	              time:hasBeginning [
	                  rdf:type time:Instant ;
	                  time:inXSDDateTimeStamp "2024-01-24"^^xsd:dateTimeStamp ;
	                ] ;
	            ] ;
	          organization:organization <http://www.ign.fr/> ;
	          organization:organization <http://www.umr-lastig.fr/> ;
	          organization:role [
	              rdf:type organization:Role ;
	              rdfs:label "Doctorante"@fr, "PhD Student"@en ;
	            ] ;
	        ] ;
	      foaf:family_name "Tual" ;
	      foaf:firstName "Solenn" ;
	      foaf:mbox <mailto:solenn.tual@ign.fr> ;
	      foaf:name "Solenn Tual" ;
          foaf:workInfoHomepage <https://www.umr-lastig.fr/solenn-tual/> ;
	    ] ; 
        dcterms:description "Ontologie décrivant le contenu des séries du cadastre napoléonien"@fr, 
		                    "Ontology describing the napoleonic cadaster documents."@en;
        dcterms:issued "2024-01-24"^^xsd:date ;
        dcterms:modified "2024-01-24"^^xsd:date ;
        dcterms:publisher <http://fr.dbpedia.org/resource/Institut_national_de_l%27information_g%C3%A9ographique_et_foresti%C3%A8re> ;
        dcterms:title "Ontologie du cadastre napoléonien"@fr, "Ontology of the Napoleonic Cadaster"@en ;
        vann:preferredNamespacePrefix "nap" ;
		vann:preferredNamespaceUri <http://data.ign.fr/def/cadastrenap#> ;
		dcterms:rights "Copyright 2024, IGN" ;
		cc:license <http://www.data.gouv.fr/Licence-Ouverte-Open-Licence> ;
		cc:license <http://creativecommons.org/licenses/by/2.0/> ;
		cc:license <http://opendatacommons.org/licenses/by/> ;
        #owl:priorVersion <> ;
		owl:versionInfo "Version 0.0 - 2024-01-24" .

# ---- Imported properties for annotation ----
skos:inScheme rdf:type owl:AnnotationProperty.
skos:prefLabel rdf:type owl:AnnotationProperty .
skos:altLabel rdf:type owl:AnnotationProperty.
skos:broader rdf:type owl:AnnotationProperty.
skos:definition rdf:type owl:AnnotationProperty.

###############################
# ---- Classes definition ----
###############################

nap:CompteFoncier a owl:Class;
    rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
    rdfs:label "Property tax account"@en , "Compte foncier"@fr ;
    skos:prefLabel "compte foncier"@fr;
    skos:altLabel "article"@fr, "folio"@fr, "case"@fr.

nap:Presence a owl:Class;
    rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
    rdfs:comment "Presence d'une parcelle ou d'un propriétaire dans un compte foncier"@fr;
    rdfs:label "Présence dans un compte foncier"@fr, "Mention in a property tax account"@en;
    skos:altLabel "Occurence".

#############################
# ---- Object properties ----
#############################

nap:isLinkedWith a owl:ObjectProperty;
    rdfs:domain nap:Landmark ;
    rdfs:range nap:Presence;
    rdfs:comment "Une parcelle est présente [dans un compte foncier]."@fr.

nap:linkedWith a owl:ObjectProperty;
    rdfs:domain nap:Presence ;
    rdfs:range nap:Owner;
    rdfs:comment "Présence d'un propriétaire [dans un compte foncier]."@fr.

nap:isMentionnedIn a owl:ObjectProperty;
    rdfs:domain nap:Presence;
    rdfs:range nap:CompteFoncier;
    rdfs:comment "Un compte foncier mentionne la présence d'une parcelle."@fr.

nap:mentions a owl:ObjectProperty;
    rdfs:domain nap:CompteFoncier;
    rdfs:range nap:Presence;
    rdfs:comment "Un compte foncier mentionne la présence d'un propriétaire."@fr.

nap:isContinuedWith a owl:ObjectProperty;
    rdfs:domain nap:CompteFoncier;
    rdfs:range nap:CompteFoncier;
    rdfs:comment "Un compte foncier X a pour suite le compte foncier Y."@fr.

###############################
# ---- Datatype properties ----
###############################

# ---- Imported properties ----
rdfs:label a owl:DatatypeProperty.

nap:hasNum a owl:DatatypeProperty;
    rdfs:domain nap:CompteFoncier ;
    rdfs:range xsd:integer;
    rdfs:label "a pour numéro de compte"@fr ,
        "has property tax account number"@en .
#################################################################
#    Classes
#################################################################

###  http://data.ign.fr/def/cadastre#Nature
cad:Nature a owl:Class ;
              rdfs:label "Nature de parcelle"@en ,
                          "Nature de parcelle"@fr .

## http://data.ign.fr/def/cadastre#Owner
cad:Owner a owl:Class;
  rdfs:label "Owner"@en , "Propriétaire ou usufruitier"@fr ;
  rdfs:comment "Propriétaire ou usufruitier : personne redevable de l'impôt foncier pour les terres dont elle a la pleine propriété ou l'usufruit."@fr .

## http://data.ign.fr/def/cadastre#OwnerAttribute
cad:OwnerAttribute a owl:Class;
  rdfs:label "Owner attribute"@en , "Attribut associé à un propriétaire ou usufruitier"@fr ;
  rdfs:comment "Attribut servant à décrire un propriétaire ou usufruitier."@fr ;
  rdfs:subClassOf add:Attribute .

## http://data.ign.fr/def/cadastre#OwnerAttributeVersion
cad:OwnerAttributeVersion a owl:Class;
  rdfs:label "Owner attribute version"@en , "Version d'un attribut associé à un propriétaire ou usufruitier"@fr ;
  rdfs:comment "Version d'un attribut servant à décrire un propriétaire ou usufruitier."@fr ;
  rdfs:subClassOf add:AttributeVersion.

## http://data.ign.fr/def/cadastre#OwnerAttributeType
cad:OwnerAttributeType a owl:Class;
  rdfs:label "Owner Attribute type"@en , "Type d'un attribut associé à un propriétaire ou usufruitier"@fr ;
  rdfs:comment "Type d'un attribut servant à décrire un propriétaire ou usufruitier."@fr ;
  rdfs:subClassOf add:AttributeType.

## http://data.ign.fr/def/cadastre#OwnerMention
cad:OwnerMention a owl:Class;
  rdfs:label "Presence of an owner in a property account"@en ,"Mention d'un propriétaire dans un compte foncier"@fr ;
  rdfs:comment "Présence d'un propriétaire dans un compte foncier."@fr.

###  http://data.ign.fr/def/cadastre#PropertyAccount
cad:PropertyAccount a owl:Class;
    rdfs:label "Property account"@en, "Compte foncier"@fr ;
    skos:prefLabel "Property account"@en, "Compte foncier"@fr;
    skos:altLabel "Article"@fr, "Folio"@fr, "Case"@fr.

###  http://data.ign.fr/def/cadastre#Folio
cad:Folio a owl:Class;
    rdfs:label "Folio"@en, "Folio"@fr ;
    skos:prefLabel "Folio"@en, "Folio"@fr ;
    skos:altLabel "Article"@fr, "Case"@fr.


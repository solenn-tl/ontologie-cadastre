@prefix cad_atype: <http://data.ign.fr/id/codes/cadastre/attributeType/> .
@prefix cad_ltype: <http://data.ign.fr/id/codes/cadastre/landmarkType/> .
@prefix cad_etype: <http://data.ign.fr/id/codes/cadastre/eventType/> .

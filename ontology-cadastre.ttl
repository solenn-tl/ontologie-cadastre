		owl:versionInfo "Version 0.0 - 2024-05-21" .

#################################################################
#    Annotation properties
#################################################################
owl:maxQualifiedCardinality a owl:AnnotationProperty .
skos:altLabel a owl:AnnotationProperty .
skos:broader a owl:AnnotationProperty .
skos:definition a owl:AnnotationProperty .
skos:inScheme a owl:AnnotationProperty .
skos:prefLabel a owl:AnnotationProperty .

#################################################################
#    Classes
#################################################################

# ---- Our classes ----
## relative to landmarks
cad:Nature a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Plot nature"@en, "Nature de parcelle"@fr ;
    rdfs:comment "Nature d'une parcelle correspondant à une occupation ou à un usage du sol"@fr ,
                 "Nature of a plot corresponding to a land use or occupation"@en .

## relative to taxpayers
cad:Taxpayer a owl:Class;
    rdfs:label "Taxpayer"@en, "contribuable"@fr ;
    skos:altLabel "Owner"@en, "Propriétaire ou usufruitier"@fr ;
    rdfs:comment "Propriétaire ou usufruitier : personne redevable de l'impôt foncier pour les terres dont elle a la pleine propriété ou l'usufruit."@fr .

cad:TaxpayerAttribute a owl:Class ;
    rdfs:label "Attribut de propriétaire"@fr .

cad:TaxpayerAttributeType a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Type d'attribut de contribuable"@fr .

cad:TaxpayerAttributeVersion a owl:Class ;
    rdfs:label "Attribute version"@en ,
               "Version de l'attribut de contribuable"@fr .

#relative to sources
cad:SourceType a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Type de source"@fr, "Type of source"@en.

cad:MLClasse a owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:label "Classe d'un document"@fr, "Classe of document"@en.

#relative to cadastre use
cad:PropertyAccount a owl:Class;
    rdfs:label "Property account"@en, "Compte foncier"@fr ;
    skos:prefLabel "Property account"@en, "Compte foncier"@fr;
    rdfs:comment "Liste des parcelles appartenant à un propriétaires.".

cad:SpecialCellValue a owl:Class;
    rdfs:subClassOf skos:Concept ;
    skos:prefLabel "Special cell value"@en, "Valeur spéciale"@fr ;
    rdfs:label "Valeur spéciale liée au fonctionnement des documents cadastraux"@fr, "Special value related to the operation of cadastral documents"@en ;
    rdfs:comment "Valeur spéciale liée au fonctionnement des documents cadastraux, par exemple les mentions de construction, destruction dans les colonnes réservées aux numéros de folio."@fr.

# ---- Imported classes ----
skos:Concept a owl:Class .
skos:ConceptScheme a owl:Class .

rico:RecordSet a owl:Class ;
	rdfs:subClassOf rico:RecordResource ;
	rdfs:comment """One or more records that are grouped together by an Agent based on
         the records sharing one or more attributes or relations."""@en ;
	rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
	rdfs:label "Record Set"@en, "Ensemble d’objets informationnels"@fr, "Agrupación documental"@es .

rico:Record a owl:Class ;
	rdfs:subClassOf rico:RecordResource ;
	rdfs:comment """Discrete information content formed and inscribed, at least once,
         by any method on any carrier in any persistent, recoverable form by an Agent in the course
         of life or work activity."""@en ;
	rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
	rdfs:label "Record"@en, "Objet informationnel"@fr, "Documento"@es .

rico:RecordPart a owl:Class ;
	rdfs:subClassOf rico:RecordResource ;
	rdfs:comment """Component of a Record with independent information content that
         contributes to the intellectual completeness of the Record."""@en ;
	rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
	rdfs:label "Record Part"@en, "Partie d’objet informationnel"@fr, "Componente documental"@es.

rico:Instantiation
  a owl:Class ;
  rdfs:subClassOf rico:Thing ;
  rdfs:comment """The inscription of information made by an Agent on a physical
         carrier in any persistent, recoverable form as a means of communicating information through
         time and space."""@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "Instantiation"@en, "Instanciation"@fr, "Instanciación"@es ;
  rico:closeTo "PREMIS Representation"@en .

prov:Activity a owl:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "Activity" ;
    owl:disjointWith prov:Entity.

prov:Agent a owl:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "Agent" ;
    owl:disjointWith prov:InstantaneousEvent.

prov:SoftwareAgent a owl:Class ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "SoftwareAgent" ;
    rdfs:subClassOf prov:Agent.

#################################################################
#    Objectype properties
#################################################################

#relative to landmarks
cad:hasPlotNature a owl:ObjectProperty ;
    rdfs:domain add:AttributeVersion ;
    rdfs:range cad:Nature ;
    rdfs:label "a pour nature"@fr ,
        "has plot nature"@en;
    owl:inverseOf cad:isPlotNatureOf.

cad:isPlotNatureOf a owl:ObjectProperty ;
    rdfs:domain cad:Nature ;
    rdfs:range add:AttributeVersion ;
    rdfs:label "est une nature de "@fr ,
                "is plot nature of"@en;
    owl:inverseOf cad:hasPlotNature.

cad:hasTaxpayer a owl:ObjectProperty ;
                    rdfs:domain add:AttributeVersion ;
                    rdfs:range cad:Taxpayer ;
                    rdfs:label "a pour contribuable"@fr ,
                               "has taxpayer"@en ;
                    owl:inverseOf cad:isTaxpayerOf.

cad:isTaxpayerOf a owl:ObjectProperty ;
                    rdfs:domain cad:Taxpayer ;
                    rdfs:range add:AttributeVersion ;
                    rdfs:label "est le contribuable associé à"@fr ,
                               "is taxpayer of"@en ;
                    owl:inverseOf cad:hasTaxpayer.

cad:hasPlotAddress a owl:ObjectProperty ;
    rdfs:domain add:AttributeVersion ;
    rdfs:range add:Address ;
    rdfs:label "a pour adresse"@fr ,
               "has address"@en ;
    owl:inverseOf add:isPlotAddressOf.

cad:isPlotAddressOf a owl:ObjectProperty ;
    rdfs:domain add:Address ;
    rdfs:range add:AttributeVersion ;
    rdfs:label "est l'adresse associée à"@fr ,
               "is address of"@en ;
    owl:inverseOf cad:hasPlotAddress.

cad:hasTaxpayerAddress a owl:ObjectProperty ;
    rdfs:domain cad:TaxpayerAttributeVersion ;
    rdfs:range add:Address ;
    rdfs:label "a pour adresse"@fr ,
               "has address"@en ;
    owl:inverseOf cad:isTaxpayerAddressOf.

cad:isTaxpayerAddressOf a owl:ObjectProperty ;
    rdfs:domain add:Address ;
    rdfs:range cad:TaxpayerAttributeVersion ;
    rdfs:label "est l'adresse du contribuable"@fr ,
               "is taxpayer address of"@en ;
    owl:inverseOf cad:hasTaxpayerAddress.

#relative to taxpayers
cad:hasTaxpayerAttribute a owl:ObjectProperty ;
    owl:inverseOf cad:isTaxpayerAttributeOf ;
    rdfs:domain cad:Taxpayer ;
    rdfs:range cad:TaxpayerAttribute ;
    rdfs:comment "Décrit l'attribut pour un contribuable"@fr ;
    rdfs:label "a pour attribut"@fr .

cad:isTaxpayerAttributeOf a owl:ObjectProperty ;
    owl:inverseOf cad:hasTaxpayerAttribute ;
    rdfs:comment "Définit le contribuable pour lequel l'attribut existe"@fr ;
    rdfs:label "est un attribut de"@fr .

cad:hasTaxpayerAttributeVersion a owl:ObjectProperty ;
    owl:inverseOf cad:isTaxpayerAttributeVersionOf ;
    rdfs:domain cad:TaxpayerAttribute ;
    rdfs:range cad:TaxpayerAttributeVersion ;
    rdfs:comment "Version d'une valeur associée à l'attribut"@fr ;
    rdfs:label "a pour version"@fr .

cad:isTaxpayerAttributeVersionOf a owl:ObjectProperty ;
    owl:inverseOf cad:hasTaxpayerAttributeVersion ;
    rdfs:comment "Attribut associée à la version d'attribut"@fr ;
    rdfs:label "est une version de l'attribut"@fr .

#relative to cadastre use
cad:takenFrom a owl:ObjectProperty ;
    rdfs:label "Tiré de"@fr,
               "Taken from"@en ;
    rdfs:comment "Numéro du folio où est décrit l'état de parcelle précédent ou l'événement qui initialise l'état considéré."@fr .

cad:passedTo a owl:ObjectProperty;
    rdfs:label "Passé à"@fr,
            "Passed to"@en ;
    rdfs:comment "Numéro du folio où est décrit l'état de parcelle suivant ou l'événement qui met fin à un état considéré."@fr .

cad:hasAssociatedFolioInMatriceDesProprietesBaties a owl:ObjectypeProperty ;
    rdfs:label "a pour folio lié dans la matrice des propriétés bâties"@fr,
               "has linked folio in the built properties matrix"@en .

cad:hasAssociatedFolioInMatriceDesProprietesNonBaties a owl:ObjectypeProperty ;
    rdfs:label "a pour folio lié dans la matrice des propriétés non bâties"@fr,
               "has linked folio in the unbuilt properties matrix"@en .

cad:hasLinkedFolioInPreviousMatrice a owl:ObjectProperty;
    rdfs:comment "Un folio X d'une matrice est la recopie/suite d'un folio Y dans la matrice précédente."@fr;
    rdfs:label "suit";
    owl:inverseOf cad:isContinuationOf.

cad:hasLinkedFolioInNextMatrice a owl:ObjectProperty;
    rdfs:comment "Un folio Y d'une matrice est précède un folio X dans la matrice suivante."@fr;
    rdfs:label "précède".

#relative to sources
cad:hasSourceType a owl:ObjectProperty ;
                    rdfs:range cad:SourceType ;
                    rdfs:comment "Décrit le type de source."@fr ;
                    rdfs:label "est une source de type"@fr ,
                               "is source type"@en .

cad:hasClasse a owl:ObjectProperty ;
                    rdfs:comment "Indique qu'un document ou d'une zone de document possède au moins une classe attribuée manuellement ou automatique."@fr ;
                    rdfs:label "a au moins une classe"@fr ,
                               "has at least one classe"@en .

cad:hasClasseValue a owl:ObjectProperty ;
                    rdfs:comment "Classe attribuée à un document ou à une zone de document par un prov:Agent."@fr ;
                    rdfs:label "a pour classe"@fr ,
                               "has classe value"@en .

cad:mentions a owl:ObjectProperty;
    rdfs:comment "Une source mentionne un objet."@fr;
    rdfs:label "mentionne";
    owl:inverseOf cad:isMentionnedIn.

cad:isMentionnedIn a owl:ObjectProperty;
    rdfs:comment "Un objet est mentionné dans une source."@fr;
    rdfs:label "est mentionné dans";
    owl:inverseOf cad:mentions.

cad:fromSource a owl:ObjectProperty .

cad:isSourceFor a owl:ObjectProperty .

# --------- Imported properties ---------
rico:hasCreationDate a owl:ObjectProperty ;
  owl:inverseOf rico:isCreationDateOf ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (
     rico:Instantiation
     rico:RecordResource
   )
  ] ;
  rdfs:comment "Inverse of 'is creation date of' object property"@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "has creation date"@en, "a pour date de création"@fr, "tiene como fecha de creación"@es .

rico:isCreationDateOf a owl:ObjectProperty ;
  owl:inverseOf rico:hasCreationDate ;
  rdfs:range [
    a owl:Class ;
    owl:unionOf (
     rico:Instantiation
     rico:RecordResource
   )
  ] ;
  rdfs:comment """Connects a Date to a Record Resource or Instantiation that was
         created at this Date."""@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "is creation date of"@en, "est la date de création de"@fr, "es fecha de creación de"@es .

rico:includesOrIncluded a owl:ObjectProperty ;
	owl:inverseOf rico:isOrWasIncludedIn ;
	rdfs:domain rico:RecordSet ;
	rdfs:range [
		rdf:type owl:Class ;
		owl:unionOf (
			rico:Record
			rico:RecordSet
		) ;
	] ;
	rdfs:comment """Connects a Record Set to a Record or Record Set it aggregates, or
         aggregated in the past."""@en ;
	rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
	rdfs:label "includes or included"@en, "inclut ou a inclus"@fr, "incluye o incluía"@es .

rico:isOrWasIncludedIn a owl:ObjectProperty ;
  owl:inverseOf rico:includesOrIncluded ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (
     rico:Record
     rico:RecordSet)] ;
  rdfs:range rico:RecordSet ;
  rdfs:comment "Inverse of 'includes or included' object property."@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "is or was included in"@en, "est ou a été inclus dans"@fr, "está o estaba incluido en"@es .

rico:hasOrHadComponent a owl:ObjectProperty ;
	owl:inverseOf rico:isOrWasComponentOf ;
	rdfs:domain rico:Instantiation ;
	rdfs:range rico:Instantiation ;
	rdfs:comment """Connects an Instantiation to one of its present or past component
         instantiations."""@en ;
	rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
	rdfs:label "has or had component"@en, "a ou a eu pour composant"@fr, "tiene o tenía como componente"@es .

rico:isOrWasComponentOf a owl:ObjectProperty ;
  owl:inverseOf rico:hasOrHadComponent ;
  rdfs:domain rico:Instantiation ;
  rdfs:range rico:Instantiation ;
  rdfs:comment "Inverse of 'has or had component' object property."@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "is or was component of"@en, "est ou a été un composant de"@fr, "es o era componente de"@es .

rico:hasOrHadInstantiation a owl:ObjectProperty ;
	owl:inverseOf rico:isOrWasInstantiationOf ;
	rdfs:domain rico:RecordResource ;
	rdfs:range rico:Instantiation ;
	rdfs:comment """Connects a Record Resource to an Instantiation, which either may
         exist or may have been lost or destroyed."""@en ;
	rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
	rdfs:label "has or had instantiation"@en, "a ou a eu pour instanciation"@fr, "tiene o tenía como instanciación"@es .

rico:isOrWasInstantiationOf a owl:ObjectProperty ;
  owl:inverseOf rico:hasOrHadInstantiation ;
  rdfs:domain rico:Instantiation ;
  rdfs:range rico:RecordResource ;
  rdfs:comment """Inverse of 'has or had instantiation' object
         property."""@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "is or was instantiation of"@en, "est ou a été une instanciation de"@fr, "es o era instanciación de"@es .

rico:hasOrHadDigitalInstantiation a owl:ObjectProperty ;
	rdfs:subPropertyOf rico:hasOrHadInstantiation ;
	owl:inverseOf rico:isOrWasDigitalInstantiationOf ;
	rdfs:domain rico:RecordResource ;
	rdfs:range rico:Instantiation ;
	rdfs:comment """Connects a record resource to one of its digital instantiations,
         whether it exists or has been lost or destroyed."""@en ;
	rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
	rdfs:label "has or had digital instantiation"@en, "a ou a eu pour instanciation numérique"@fr, "tiene o tenía como instanciación digital"@es .

rico:isOrWasDigitalInstantiationOf a owl:ObjectProperty ;
  rdfs:subPropertyOf rico:isOrWasInstantiationOf ;
  owl:inverseOf rico:hasOrHadDigitalInstantiation ;
  rdfs:domain rico:Instantiation ;
  rdfs:range rico:RecordResource ;
  rdfs:comment """Inverse of 'has or had digital instantiation' object
         property."""@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "is or was digital instantiation of"@en, "est ou a été une instanciation numérique de"@fr, "es o era instanciación digital de"@es .

rico:hasOrHadComponent
  a owl:ObjectProperty ;
  rdfs:subPropertyOf rico:hasOrHadPart, rico:isInstantiationAssociatedWithInstantiation ;
  owl:inverseOf rico:isOrWasComponentOf ;
  rdfs:domain rico:Instantiation ;
  rdfs:range rico:Instantiation ;
  rdfs:comment """Connects an Instantiation to one of its present or past component
         instantiations."""@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "has or had component"@en, "a ou a eu pour composant"@fr, "tiene o tenía como componente"@es .

rico:hasOrHadConstituent
  a owl:ObjectProperty ;
  owl:inverseOf rico:isOrWasConstituentOf ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (
     rico:Record
     rico:RecordPart
   )
  ] ;
  rdfs:range [
    a owl:Class ;
    owl:unionOf (
     rico:Record
     rico:RecordPart
   )
  ] ;
  rdfs:comment """Connects a Record or Record Part to a Record or Record part that
         is or was a constituent of that Record or Record Part."""@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "has or had constituent"@en, "a ou a eu pour constituant"@fr, "tiene o tenía como elemento constitutivo"@es .

rico:isOrWasConstituentOf
  a owl:ObjectProperty ;
  rdfs:subPropertyOf rico:isOrWasPartOf, rico:isRecordResourceAssociatedWithRecordResource ;
  owl:inverseOf rico:hasOrHadConstituent ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (
     rico:Record
     rico:RecordPart
   )
  ] ;
  rdfs:range [
    a owl:Class ;
    owl:unionOf (
     rico:Record
     rico:RecordPart
   )
  ] ;
  rdfs:comment """Inverse of 'has or had constituent' object
         property."""@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "is or was constituent of"@en, "est ou a été un constituant de"@fr, "es o era elemento constitutivo de"@es .

rico:hasComponentTransitive
  a owl:ObjectProperty, owl:TransitiveProperty ;
  rdfs:subPropertyOf rico:hasOrHadComponent, rico:hasPartTransitive ;
  owl:inverseOf rico:isComponentOfTransitive ;
  rdfs:domain rico:Instantiation ;
  rdfs:range rico:Instantiation ;
  rdfs:comment """Connects an Instantiation to another Instantiation that is,
         directly or indirectly, a component of that Instantiation. This is a transitive
         relation."""@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "has component transitive"@en, "a pour composant transitif"@fr, "tiene como componente transitivo"@es .

rico:hasConstituentTransitive
  a owl:ObjectProperty, owl:TransitiveProperty ;
  rdfs:subPropertyOf rico:hasOrHadConstituent, rico:hasPartTransitive ;
  owl:inverseOf rico:isConstituentOfTransitive ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (
     rico:Record
     rico:RecordPart
   )
  ] ;
  rdfs:range [
    a owl:Class ;
    owl:unionOf (
     rico:Record
     rico:RecordPart
   )
  ] ;
  rdfs:comment """Connects a Record or Record Part to another Record or Record Part
         that is its constituent, directly or indirectly. This is a transitive
         relation."""@en ;
  rdfs:isDefinedBy <https://www.ica.org/standards/RiC/ontology> ;
  rdfs:label "has constituent transitive"@en, "a pour constituant transitif"@fr, "tiene como elemento constitutivo transitivo"@es ;.

prov:wasAssociatedWith a owl:ObjectProperty ;
    rdfs:comment "An prov:Agent that had some (unspecified) responsibility for the occurrence of this prov:Activity."@en ;
    rdfs:domain prov:Activity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "wasAssociatedWith" ;
    rdfs:range prov:Agent ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    owl:propertyChainAxiom (prov:qualifiedAssociation
        prov:agent
    ).

prov:used a owl:ObjectProperty ;
    rdfs:comment "A prov:Entity that was used by this prov:Activity. For example, :baking prov:used :spoon, :egg, :oven ."@en ;
    rdfs:domain prov:Activity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "used" ;
    rdfs:range prov:Entity ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    owl:propertyChainAxiom (prov:qualifiedUsage
        prov:Entity
    ).

prov:wasGeneratedBy a owl:ObjectProperty ;
    rdfs:domain prov:Entity ;
    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
    rdfs:label "wasGeneratedBy" ;
    rdfs:range prov:Activity ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    owl:propertyChainAxiom (prov:qualifiedGeneration
        prov:activity
    ).


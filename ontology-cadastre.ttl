#################################################################
#    Object Properties
#################################################################

###  http://data.ign.fr/def/cadastre#isMadeEffectiveBy
cad:isMadeEffectiveBy a owl:ObjectProperty ;
                    owl:inverseOf add:makesEffective ;
                    rdfs:domain add:AttributeChange ;
                    rdfs:range add:AttributeVersion ;
                    rdfs:comment "Décrit le fait qu'une version d'un attribut est rendue effective par un changement"@fr ;
                    rdfs:label "est rendu effectif par"@fr .

###  http://data.ign.fr/def/cadastre#isMadeOutdatedBy
cad:isMadeOutdatedBy a owl:ObjectProperty ;
              owl:inverseOf add:outdates ;
              rdfs:domain add:AttributeChange ;
              rdfs:range add:AttributeVersion ;
              rdfs:comment "Décrit le fait qu'une version d'un attribut est rendu caduc par un changement"@fr ;
              rdfs:label "est rendu caduc"@fr .

###  http://data.ign.fr/def/cadastre#hasPlotNature
cad:hasPlotNature a owl:ObjectProperty ;
                    rdfs:domain add:AttributeVersion ;
                    rdfs:range cad:Nature ;
                    rdfs:label "a pour nature"@fr ,
                               "has plot nature"@en;
                    owl:inverseOf cad:isPlotNatureOf.

###  http://data.ign.fr/def/cadastre#isPlotNature
cad:isPlotNatureOf a owl:ObjectProperty ;
                    rdfs:domain cad:Nature ;
                    rdfs:range add:AttributeVersion ;
                    rdfs:label "est une nature de "@fr ,
                               "is plot nature of"@en;
                    owl:inverseOf cad:hasPlotNature.

###  http://data.ign.fr/def/cadastre#hasOwner
cad:hasOwner a owl:ObjectProperty ;
                    rdfs:domain add:AttributeVersion ;
                    rdfs:range cad:Owner ;
                    rdfs:label "a pour propriétaire"@fr ,
                               "has owner"@en ;
                    owl:inverseOf cad:isOwnerOf.

###  http://data.ign.fr/def/cadastre#isOwnedBy
cad:isOwnerOf a owl:ObjectProperty ;
                    rdfs:domain cad:Owner ;
                    rdfs:range add:AttributeVersion ;
                    rdfs:label "est le propriétaire de"@fr ,
                               "is owner of"@en ;
                    owl:inverseOf cad:hasOwner.

###  http://data.ign.fr/def/cadastre#hasOwnerAttribute
cad:hasOwnerAttribute a owl:ObjectProperty ;
                    rdfs:domain cad:Owner ;
                    rdfs:domain cad:OwnerAttribute ;
                    rdfs:label "a pour attribut de propriétaire"@fr ,
                               "has owner attribute"@en;
                    rdfs:comment "Attribut de propriétaire.".

###  http://data.ign.fr/def/cadastre#hasOwnerAttributeType
cad:hasOwnerAttributeType a owl:ObjectProperty ;
                    rdfs:domain cad:OwnerAttribute ;
                    rdfs:domain cad:OwnerAttributeType ;
                    rdfs:label "a pour type d'attribut de propriétaire"@fr ,
                               "has owner attribute type"@en;
                    rdfs:comment "Type de l'attribut de propriétaire.".

###  http://data.ign.fr/def/cadastre#hasOwnerAttributeType
cad:hasOwnerAttributeVersion a owl:ObjectProperty ;
                    rdfs:domain cad:OwnerAttribute ;
                    rdfs:domain cad:OwnerAttributeVersion ;
                    rdfs:label "a pour version d'attribut de propriétaire"@fr ,
                               "has version of owner attribute"@en;
                    rdfs:comment "Version d'un attribut de propriétaire.".

cad:hasOwnerAttributeValue a owl:ObjectProperty ;
              rdfs:domain cad:OwnerAttributeVersion ;
              rdfs:label "a pour version d'attribut de propriétaire"@fr ,
                               "has version of owner attribute"@en;
              rdfs:comment "Version d'un attribut de propriétaire.".

###  http://data.ign.fr/def/cadastre#mentions
cad:mentions a owl:ObjectProperty;
    #rdfs:domain cad:PropertyAccount; domaine = PropertyAccount ou Folio
    rdfs:range cad:AttributeVersion;
    rdfs:comment "Un folio/compte foncier mentionne la présence d'une parcelle ou d'un propriétaire."@fr;
    rdfs:label "mentionne";
    owl:inverseOf cad:isMentionnedIn.

###  http://data.ign.fr/def/cadastre#isMentionnedIn
cad:isMentionnedIn a owl:ObjectProperty;
    rdfs:domain cad:AttributeVersion;
    rdfs:range cad:PropertyAccount;
    rdfs:comment "Une parcelle ou un propriétaire est mentionné dans un folio/compte foncier."@fr;
    rdfs:label "est mentionné dans";
    owl:inverseOf cad:mentions.

###  http://data.ign.fr/def/cadastre#continuedAt
cad:continuedAt a owl:ObjectProperty;
    rdfs:domain cad:PropertyAccount;
    rdfs:range cad:PropertyAccount;
    rdfs:comment "Un compte foncier X se poursuit dans le compte foncier Y (généralement si le compte s'étend sur plusieurs pages)."@fr;
    rdfs:label "poursuivi au ";
    owl:inverseOf cad:isContinuationOf.

###  http://data.ign.fr/def/cadastre#isContinuationOf
cad:isContinuationOf a owl:ObjectProperty;
    rdfs:domain cad:PropertyAccount;
    rdfs:range cad:PropertyAccount;
    rdfs:comment "Un  compte foncier Y est la suite d'un compte foncier X (généralement si le compte s'étend sur plusieurs pages)."@fr;
    rdfs:label "est la suite du ";
    owl:inverseOf cad:continuedAt.

###  http://data.ign.fr/def/cadastrenap#isPropertyAccountEquivalentOf
cad:isPropertyAccountEquivalentOf a owl:ObjectProperty ;
                    rdfs:domain cad:PropertyAccount;
                    rdfs:range rico:RecordPart ;
                    rdfs:comment "Indique le compte foncier auquel est lié un record"@fr ;
                    rdfs:label "est le compte foncier équivalent à"@fr ,
                               "is property account equivalent of"@en ;
                    owl:inverseOf cad:isRecordEquivalentOf.

###  http://data.ign.fr/def/cadastrenap#isRecordEquivalentOf
cad:isRecordEquivalentOf a owl:ObjectProperty ;
                    rdfs:domain rico:RecordPart;
                    rdfs:range cad:PropertyAccount;
                    rdfs:comment "Indique le Record Part auquel est lié un compte foncier"@fr ;
                    rdfs:label "est le Record Part équivalent à"@fr ,
                               "is record part equivalent of"@en ;
                    owl:inverseOf cad:isPropertyAccountEquivalentOf.

###  http://data.ign.fr/def/cadastrenap#existsIn
cad:existsIn a owl:ObjectProperty ;
                    rdfs:domain cad:PropertyAccount;
                    rdfs:range rico:RecordSet;
                    rdfs:comment "Indique qu'un compte foncier existe dans un registre (en tant que concept) donné"@fr ;
                    rdfs:label "existe dans"@fr ,
                               "exists in"@en .


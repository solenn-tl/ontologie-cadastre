@prefix : <http://rdf.geohistoricaldata.org/def/address#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix atype: <http://rdf.geohistoricaldata.org/id/codes/address/attributeType/> .
@prefix ctype: <http://rdf.geohistoricaldata.org/id/codes/address/changeType/> .
@prefix ltype: <http://rdf.geohistoricaldata.org/id/codes/address/landmarkType/> .
@prefix lrtype: <http://rdf.geohistoricaldata.org/id/codes/address/landmarkRelationType/> .
@base <http://rdf.geohistoricaldata.org/def/address#> .

<http://rdf.geohistoricaldata.org/def/address> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2002/07/owl#maxQualifiedCardinality
owl:maxQualifiedCardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#broader
skos:broader rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#inScheme
skos:inScheme rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://rdf.geohistoricaldata.org/def/address#after
:after rdf:type owl:ObjectProperty ;
       owl:inverseOf :before ;
       rdfs:domain :Change ;
       rdfs:range :Change ;
       rdfs:comment "Décrit la relation temporelle de postériorité entre deux changements"@fr ;
       rdfs:label "est après"@fr .


###  http://rdf.geohistoricaldata.org/def/address#appliedTo
:appliedTo rdf:type owl:ObjectProperty ;
           owl:inverseOf :changedBy ;
           rdfs:domain :Change ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :Attribute
                                      :Landmark
                                      :LandmarkRelation
                                    )
                      ] ;
           rdfs:comment "Définit sur quoi s'applique le changement"@fr ;
           rdfs:label "s'applique"@fr .


###  http://rdf.geohistoricaldata.org/def/address#before
:before rdf:type owl:ObjectProperty ;
        rdfs:domain :Change ;
        rdfs:range :Change ;
        rdfs:comment "Décrit la relation temporelle de précédence entre deux changements"@fr ;
        rdfs:label "est avant"@fr .


###  http://rdf.geohistoricaldata.org/def/address#changedBy
:changedBy rdf:type owl:ObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Attribute
                                       :Landmark
                                       :LandmarkRelation
                                     )
                       ] ;
           rdfs:range :Change ;
           rdfs:comment "Définit le changement qui est appliqué à l'élément"@fr ;
           rdfs:label "changé par"@fr .


###  http://rdf.geohistoricaldata.org/def/address#dependsOn
:dependsOn rdf:type owl:ObjectProperty ;
           owl:inverseOf :hasChange ;
           rdfs:domain :Change ;
           rdfs:range :Event ;
           rdfs:comment "Décrit l'appartenance d'un changement à un événement"@fr ;
           rdfs:label "dépend de l'événement"@fr .


###  http://rdf.geohistoricaldata.org/def/address#firstStep
:firstStep rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasStep ;
           rdfs:domain :Address ;
           rdfs:range :AddressSegment ;
           rdfs:comment "Première étape de l'adresse."@fr ;
           rdfs:label "a pour première étape"@fr ,
                      "has first step"@en ;
           skos:altLabel "a pour premier segment d'adresse"@fr .


###  http://rdf.geohistoricaldata.org/def/address#hasAttribute
:hasAttribute rdf:type owl:ObjectProperty ;
              owl:inverseOf :isAttributeOf ;
              rdfs:domain :Landmark ;
              rdfs:range :Attribute ;
              rdfs:comment "Décrit l'attribut pour un repère"@fr ;
              rdfs:label "a pour attribut"@fr .


###  http://rdf.geohistoricaldata.org/def/address#hasAttributeVersion
:hasAttributeVersion rdf:type owl:ObjectProperty ;
                     owl:inverseOf :isAttributeVersionOf ;
                     rdfs:domain :Attribute ;
                     rdfs:range :AttributeVersion ;
                     rdfs:comment "Version d'une valeur associée à l'attribut"@fr ;
                     rdfs:label "a pour version"@fr .


###  http://rdf.geohistoricaldata.org/def/address#hasBeginning
:hasBeginning rdf:type owl:ObjectProperty ;
              rdfs:domain :TimeInterval ;
              rdfs:range :TimeInstant ;
              rdfs:comment "Début de l'intervalle temporel"@fr ;
              rdfs:label "commence par"@fr .


###  http://rdf.geohistoricaldata.org/def/address#hasChange
:hasChange rdf:type owl:ObjectProperty ;
           rdfs:domain :Event ;
           rdfs:range :Change ;
           rdfs:comment "Définit les changements qui sont liés à l'événement"@fr ;
           rdfs:label "comporte le changement"@fr ,
                      "has change"@en .


###  http://rdf.geohistoricaldata.org/def/address#hasEnd
:hasEnd rdf:type owl:ObjectProperty ;
        rdfs:domain :TimeInterval ;
        rdfs:range :TimeInstant ;
        rdfs:comment "Fin de l'intervalle temporel"@fr ;
        rdfs:label "finit par"@fr .


###  http://rdf.geohistoricaldata.org/def/address#hasFuzzyBeggining
:hasFuzzyBeggining rdf:type owl:ObjectProperty ;
                   rdfs:domain :FuzzyTimeInstant ;
                   rdfs:range :CrispTimeInstant ;
                   rdfs:comment "Définit le début de validité pour un instant flou"@fr ;
                   rdfs:label "a pour début flou"@fr .


###  http://rdf.geohistoricaldata.org/def/address#hasFuzzyEnd
:hasFuzzyEnd rdf:type owl:ObjectProperty ;
             rdfs:domain :FuzzyTimeInstant ;
             rdfs:range :CrispTimeInstant ;
             rdfs:comment "Définit la fin de validité pour un instant flou"@fr ;
             rdfs:label "a pour fin floue"@fr .


###  http://rdf.geohistoricaldata.org/def/address#hasFuzzyPeak
:hasFuzzyPeak rdf:type owl:ObjectProperty ;
              rdfs:domain :FuzzyTimeInstant ;
              rdfs:range :CrispTimeInstant ;
              rdfs:comment "Définit le pic de validité pour un instant flou"@fr ;
              rdfs:label "a pour pic flou"@fr .


###  http://rdf.geohistoricaldata.org/def/address#hasStep
:hasStep rdf:type owl:ObjectProperty ;
         owl:inverseOf :isStepOf ;
         rdf:type owl:TransitiveProperty ;
         rdfs:domain :Address ;
         rdfs:range :AddressSegment ;
         owl:propertyChainAxiom ( :firstStep
                                  :nextStep
                                ) ,
                                ( :hasStep
                                  :nextStep
                                ) ;
         rdfs:label "a pour étape"@fr ,
                    "has step"@en .


###  http://rdf.geohistoricaldata.org/def/address#hasTime
:hasTime rdf:type owl:ObjectProperty ;
         rdfs:range :TemporalEntity ;
         rdfs:comment "Valeur temporelle liée à une ressource"@fr ;
         rdfs:label "a pour valeur temporelle"@fr .


###  http://rdf.geohistoricaldata.org/def/address#isAttributeOf
:isAttributeOf rdf:type owl:ObjectProperty ;
               rdfs:comment "Définit le repère pour lequel l'attribut existe"@fr ;
               rdfs:label "est un attribut de"@fr .


###  http://rdf.geohistoricaldata.org/def/address#isAttributeType
:isAttributeType rdf:type owl:ObjectProperty ;
                 rdfs:domain :Attribute ;
                 rdfs:range :AttributeType ;
                 rdfs:comment "Décrit le type d'attribut."@fr ;
                 rdfs:label "est un attribut de type"@fr .


###  http://rdf.geohistoricaldata.org/def/address#isAttributeVersionOf
:isAttributeVersionOf rdf:type owl:ObjectProperty ;
                      rdfs:domain :AttributeVersion ;
                      rdfs:range :Attribute ;
                      rdfs:comment "Attribut associée à la version d'attribut"@fr ;
                      rdfs:label "est une version de l'attribut"@fr .


###  http://rdf.geohistoricaldata.org/def/address#isChangeType
:isChangeType rdf:type owl:ObjectProperty ;
              rdfs:domain :Change ;
              rdfs:range :ChangeType ;
              rdfs:comment "Décrit le type de changement."@fr ;
              rdfs:label "est une changement de type"@fr .


###  http://rdf.geohistoricaldata.org/def/address#isLandmarkRelationType
:isLandmarkRelationType rdf:type owl:ObjectProperty ;
                        rdfs:domain :LandmarkRelation ;
                        rdfs:range :LandmarkRelationType ;
                        rdfs:comment "Décrit le type de relation entre repères définie par la ressource."@fr ;
                        rdfs:label "est une relation entre repères de type"@fr .


###  http://rdf.geohistoricaldata.org/def/address#isLandmarkType
:isLandmarkType rdf:type owl:ObjectProperty ;
                rdfs:domain :Landmark ;
                rdfs:range :LandmarkType ;
                rdfs:comment "Décrit le type de repère définie par la ressource."@fr ;
                rdfs:label "est un repère de type"@fr ,
                           "is landmark type"@en .


###  http://rdf.geohistoricaldata.org/def/address#isStepOf
:isStepOf rdf:type owl:ObjectProperty ;
          rdfs:domain :AddressSegment ;
          rdfs:range :Address ;
          rdfs:label "est une étape de"@fr ,
                     "is a step of"@en .


###  http://rdf.geohistoricaldata.org/def/address#locatum
:locatum rdf:type owl:ObjectProperty ;
         rdfs:domain :LandmarkRelation ;
         rdfs:range :Landmark ;
         rdfs:comment "Donne le repère qui est le locatum pour la relation entre repères"@fr ,
                      "Donne le repère qui est le locatum pour la relation spatiale"@fr ;
         rdfs:label "a pour locatum"@fr ,
                    "has locatum"@en .


###  http://rdf.geohistoricaldata.org/def/address#makesEffective
:makesEffective rdf:type owl:ObjectProperty ;
                rdfs:domain :AttributeChange ;
                rdfs:range :AttributeVersion ;
                rdfs:comment "Décrit le fait qu'un changement rend effectif une version d'un attribut"@fr ;
                rdfs:label "rend effectif"@fr .


###  http://rdf.geohistoricaldata.org/def/address#nextStep
:nextStep rdf:type owl:ObjectProperty ;
          rdfs:domain :AddressSegment ;
          rdfs:range :AddressSegment ;
          rdfs:label "a pour prochaine étape"@fr ,
                     "has next step"@en ;
          skos:altLabel "est suivi par"@fr ,
                        "is followed by"@en .


###  http://rdf.geohistoricaldata.org/def/address#outdates
:outdates rdf:type owl:ObjectProperty ;
          rdfs:domain :AttributeChange ;
          rdfs:range :AttributeVersion ;
          rdfs:comment "Décrit le fait qu'un changement rend caduc une version d'un attribut"@fr ;
          rdfs:label "rend caduc"@fr .


###  http://rdf.geohistoricaldata.org/def/address#relatum
:relatum rdf:type owl:ObjectProperty ;
         rdfs:domain :LandmarkRelation ;
         rdfs:range :Landmark ;
         rdfs:comment "Donne le repère qui est un des relatums pour la relation entre repères"@fr ,
                      "Donne le repère qui est un des relatums pour la relation spatiale"@fr ;
         rdfs:label "a pour relatum"@fr ,
                    "has relatum"@en .


###  http://rdf.geohistoricaldata.org/def/address#sameTime
:sameTime rdf:type owl:ObjectProperty ,
                   owl:SymmetricProperty ;
          rdfs:domain :Change ;
          rdfs:range :Change ;
          rdfs:comment "Décrit la relation temporelle de simultanéité entre deux changements"@fr ;
          rdfs:label "est en même temps"@fr .


###  http://rdf.geohistoricaldata.org/def/address#targets
:targets rdf:type owl:ObjectProperty ;
         rdfs:domain :Address ;
         rdfs:range :Landmark ;
         rdfs:comment "Cible le repère décrivant l'adresse."@fr ;
         rdfs:label "cible"@fr ,
                    "targets"@en .


###  http://rdf.geohistoricaldata.org/def/address#timeCalendar
:timeCalendar rdf:type owl:ObjectProperty ;
              rdfs:domain :CrispTimeInstant ;
              rdfs:range time:TRS ;
              rdfs:comment "Calendrier lié à l'instant (grégorien, julien, révolutionnaire)"@fr ;
              rdfs:label "a pour calendrier"@fr .


###  http://rdf.geohistoricaldata.org/def/address#timePrecision
:timePrecision rdf:type owl:ObjectProperty ;
               rdfs:domain :CrispTimeInstant ;
               rdfs:range time:TemporalUnit ;
               rdfs:comment "Précision de l'instant (au jour, au mois, à l'année...)"@fr ;
               rdfs:label "a pour précision temporelle"@fr .


###  http://rdf.geohistoricaldata.org/def/address#versionOf
:versionOf rdf:type owl:ObjectProperty ;
           rdfs:domain :LandmarkVersion ;
           rdfs:range :Landmark ;
           rdfs:comment "Définit pour une version de quel repère elle dépend"@fr ;
           rdfs:label "est une version de repère de"@fr .


###  http://www.w3.org/ns/prov#wasDerivedFrom
prov:wasDerivedFrom rdf:type owl:ObjectProperty ;
                    rdfs:domain prov:Entity ;
                    rdfs:range prov:Entity ;
                    rdfs:comment "The more specific subproperties of prov:wasDerivedFrom (i.e., prov:wasQuotedFrom, prov:wasRevisionOf, prov:hadPrimarySource) should be used when applicable."@en ;
                    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
                    rdfs:label "est dérivé de"@fr ,
                               "wasDerivedFrom"@en .


#################################################################
#    Data properties
#################################################################

###  http://rdf.geohistoricaldata.org/def/address#timeStamp
:timeStamp rdf:type owl:DatatypeProperty ;
           rdfs:domain :CrispTimeInstant ;
           rdfs:range xsd:dateTimeStamp ;
           rdfs:comment "Timestamp lié à l'instant"@fr ;
           rdfs:label "a pour timestamp"@fr .


###  http://rdf.geohistoricaldata.org/def/address#versionValue
:versionValue rdf:type owl:DatatypeProperty ;
              rdfs:domain :AttributeVersion ;
              rdfs:comment "Valeur de liée à une version d'attribut"@fr ;
              rdfs:label "a pour valeur"@fr .


#################################################################
#    Classes
#################################################################

###  http://rdf.geohistoricaldata.org/def/address#Address
:Address rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :firstStep ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :AddressSegment
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :targets ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :Landmark
                         ] ;
         rdfs:label "address"@en ,
                    "adresse"@fr .


###  http://rdf.geohistoricaldata.org/def/address#AddressSegment
:AddressSegment rdf:type owl:Class ;
                rdfs:subClassOf :LandmarkRelation ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :relatum ;
                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :Landmark
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :locatum ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :Landmark
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :nextStep ;
                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :AddressSegment
                                ] ;
                rdfs:label "address segment"@en ,
                           "segment d'adresse"@fr .


###  http://rdf.geohistoricaldata.org/def/address#Attribute
:Attribute rdf:type owl:Class ;
           rdfs:label "Attribut"@fr .


###  http://rdf.geohistoricaldata.org/def/address#AttributeChange
:AttributeChange rdf:type owl:Class ;
                 rdfs:subClassOf :Change ;
                 rdfs:label "Change on an attribute"@en ,
                            "Changement sur l'attribut"@fr .


###  http://rdf.geohistoricaldata.org/def/address#AttributeType
:AttributeType rdf:type owl:Class ;
               rdfs:subClassOf skos:Concept ;
               rdfs:label "Type d'attribut"@fr .


###  http://rdf.geohistoricaldata.org/def/address#AttributeVersion
:AttributeVersion rdf:type owl:Class ;
                  rdfs:label "Attribute version"@en ,
                             "Version de l'attribut"@fr .


###  http://rdf.geohistoricaldata.org/def/address#Change
:Change rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :appliedTo ;
                          owl:allValuesFrom [ rdf:type owl:Class ;
                                              owl:unionOf ( :Attribute
                                                            :Landmark
                                                            :LandmarkRelation
                                                          )
                                            ]
                        ] ;
        rdfs:label "Changement"@fr .


###  http://rdf.geohistoricaldata.org/def/address#ChangeType
:ChangeType rdf:type owl:Class ;
            rdfs:subClassOf skos:Concept ;
            rdfs:label "Type de changement"@fr .


###  http://rdf.geohistoricaldata.org/def/address#CrispTemporalEntity
:CrispTemporalEntity rdf:type owl:Class ;
                     rdfs:subClassOf :TemporalEntity ;
                     rdfs:label "Crisp temporal entity"@en ,
                                "Entité temporelle non floue"@fr .


###  http://rdf.geohistoricaldata.org/def/address#CrispTimeInstant
:CrispTimeInstant rdf:type owl:Class ;
                  rdfs:subClassOf :CrispTemporalEntity ,
                                  :TimeInstant ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :timeCalendar ;
                                    owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass time:TRS
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :timePrecision ;
                                    owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass time:TemporalUnit
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :timeStamp ;
                                    owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onDataRange xsd:dateTimeStamp
                                  ] ;
                  rdfs:label "Crisp time instant"@en ,
                             "Instant temporel précis"@fr .


###  http://rdf.geohistoricaldata.org/def/address#CrispTimeInterval
:CrispTimeInterval rdf:type owl:Class ;
                   rdfs:subClassOf :CrispTemporalEntity ,
                                   :TimeInterval ;
                   rdfs:label "Crisp time interval"@en ,
                              "Intervalle temporel précis"@fr .


###  http://rdf.geohistoricaldata.org/def/address#Event
:Event rdf:type owl:Class ;
       rdfs:comment "Événément décrivant une évolution impliquant une ou plusieurs ressources à un instant donné."@fr ;
       rdfs:label "Event"@en ,
                  "Événément"@fr .


###  http://rdf.geohistoricaldata.org/def/address#FinalAddressSegment
:FinalAddressSegment rdf:type owl:Class ;
                     rdfs:subClassOf :AddressSegment ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :nextStep ;
                                       owl:cardinality "0"^^xsd:nonNegativeInteger
                                     ] ;
                     rdfs:label "final address segment"@en ,
                                "segment d'adresse final"@fr .


###  http://rdf.geohistoricaldata.org/def/address#FuzzyTemporalEntity
:FuzzyTemporalEntity rdf:type owl:Class ;
                     rdfs:subClassOf :TemporalEntity ;
                     rdfs:label "Entité temporelle floue"@fr ,
                                "Fuzzy temporal entity"@en .


###  http://rdf.geohistoricaldata.org/def/address#FuzzyTimeInstant
:FuzzyTimeInstant rdf:type owl:Class ;
                  rdfs:subClassOf :FuzzyTemporalEntity ,
                                  :TimeInstant ;
                  rdfs:label "Fuzzy time instant"@en ,
                             "Instant temporel flou"@fr .


###  http://rdf.geohistoricaldata.org/def/address#FuzzyTimeInterval
:FuzzyTimeInterval rdf:type owl:Class ;
                   rdfs:subClassOf :FuzzyTemporalEntity ,
                                   :TimeInterval ;
                   rdfs:label "Fuzzy time interval"@en ,
                              "Intervalle temporel flou"@fr .


###  http://rdf.geohistoricaldata.org/def/address#Landmark
:Landmark rdf:type owl:Class ;
          rdfs:label "Landmark"@en ,
                     "Repère"@fr ,
                     "repère"@fr .


###  http://rdf.geohistoricaldata.org/def/address#LandmarkChange
:LandmarkChange rdf:type owl:Class ;
                rdfs:subClassOf :Change ;
                rdfs:label "Change on a landmark"@en ,
                           "Changement sur le repère"@fr .


###  http://rdf.geohistoricaldata.org/def/address#LandmarkRelation
:LandmarkRelation rdf:type owl:Class ;
                  rdfs:label "Landmark relation"@en ,
                             "Relation entre repères"@fr .


###  http://rdf.geohistoricaldata.org/def/address#LandmarkRelationChange
:LandmarkRelationChange rdf:type owl:Class ;
                        rdfs:subClassOf :Change ;
                        rdfs:label "Change on a landmark relation"@en ,
                                   "changement sur la relation entre repères"@fr .


###  http://rdf.geohistoricaldata.org/def/address#LandmarkRelationType
:LandmarkRelationType rdf:type owl:Class ;
                      rdfs:subClassOf skos:Concept ;
                      rdfs:label "Landmark relation type"@en ,
                                 "Type de relation entre repères"@fr ,
                                 "type de relation entre repères"@fr ,
                                 "type of landmark relation"@en .


###  http://rdf.geohistoricaldata.org/def/address#LandmarkType
:LandmarkType rdf:type owl:Class ;
              rdfs:subClassOf skos:Concept ;
              rdfs:label "Type de repère"@fr ,
                         "Type of landmark"@en .


###  http://rdf.geohistoricaldata.org/def/address#LandmarkVersion
:LandmarkVersion rdf:type owl:Class ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty :hasTime ;
                                   owl:allValuesFrom :TimeInterval
                                 ] ;
                 rdfs:label "Landmark version"@en ,
                            "Version de repère"@fr .


###  http://rdf.geohistoricaldata.org/def/address#TemporalEntity
:TemporalEntity rdf:type owl:Class ;
                rdfs:label "Entité temporelle"@fr ,
                           "Temporal entity"@en .


###  http://rdf.geohistoricaldata.org/def/address#TimeInstant
:TimeInstant rdf:type owl:Class ;
             rdfs:subClassOf :TemporalEntity ;
             rdfs:label "Instant"@fr ,
                        "Time instant"@en .


###  http://rdf.geohistoricaldata.org/def/address#TimeInterval
:TimeInterval rdf:type owl:Class ;
              rdfs:subClassOf :TemporalEntity ;
              rdfs:label "Intervalle temporel"@fr ,
                         "Time interval"@en .


###  http://www.w3.org/2004/02/skos/core#Concept
skos:Concept rdf:type owl:Class .


###  http://www.w3.org/2004/02/skos/core#ConceptScheme
skos:ConceptScheme rdf:type owl:Class .


###  http://www.w3.org/2006/time#TRS
time:TRS rdf:type owl:Class .


###  http://www.w3.org/2006/time#TemporalUnit
time:TemporalUnit rdf:type owl:Class .


###  http://www.w3.org/ns/prov#Entity
prov:Entity rdf:type owl:Class ;
            rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
            rdfs:label "Entity" .


#################################################################
#    Individuals
#################################################################

###  http://rdf.geohistoricaldata.org/def/address#AttributeList
:AttributeList rdf:type owl:NamedIndividual ,
                        skos:ConceptScheme ;
               rdfs:comment "Liste des différents rôles pouvant être joués par un attribut, représentés comme une liste de concepts SKOS."@fr ;
               rdfs:label "Type d'attribut"@fr .


###  http://rdf.geohistoricaldata.org/def/address#ChangeList
:ChangeList rdf:type owl:NamedIndividual ,
                     skos:ConceptScheme ;
            rdfs:comment "Liste des différents rôles pouvant être joués par un changement, représentés comme une liste de concepts SKOS."@fr ;
            rdfs:label "Type d'événement"@fr .


###  http://rdf.geohistoricaldata.org/def/address#LandmarkList

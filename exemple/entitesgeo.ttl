#http://data.ign.fr/id/cadastrenap/landmark/COUNTRY_France
landmark:COUNTRY_France a nap:Landmark;
    rdfs:label "France"@fr.
    nap:isLandmarkType ltype:Country.

#http://data.ign.fr/id/cadastrenap/landmark/DEPT_Seine_et_Oise
landmark:DEPT_Seine_et_Oise a nap:Landmark;
    rdfs:label "Seine-et-Oise"@fr;
    skos:altLabel "Département de Seine-et-Oise";
    nap:isLandmarkType ltype:Departement;
    nap:hasAttribute [ a nap:Attribute;
        nap:isAttributeType atype:Name;
        nap:hasAttributeVersion [
            nap:versionValue "Seine-et-Oise"
        ]
    ];
    nap:hasAttribute[ a nap:Attribute;
        nap:isAttributeType atype:SpatialDescription;
        nap:hasAttributeVersion [ a nap:AttributeVersion;
            nap:targets landmark:DEPT_Seine_et_Oise;
            nap:firstStep [
                a nap:Segment;
                nap:isLandmarkRelationType lrtype:Within;
                nap:locatum landmark:DEPT_Seine_et_Oise;
                nap:relatum landmark:COUNTRY_France
            ];
        ]
    ]
    .

    _:ev0 a nap:Event;
        rdfs:label "Création des départements français.";
        nap:hasTime [ a nap:TimeInstant ;
            nap:timeCalendar time:Gregorian ;
            nap:timePrecision time:Day ;
            nap:timeStamp "14-03-1790"^^xsd:dateTimeStamp
        ];
        fpo:sourcedFrom "Wikipédia"
    .

    _:ev1 a nap:Event;
        rdfs:label "Décret d'application du 1er janvier 1968 sur la réorganisation de la région parienne.";
        nap:hasTime [ a nap:TimeInstant ;
            nap:timeCalendar time:Gregorian ;
            nap:timePrecision time:Day ;
            nap:timeStamp "01-01-1968"^^xsd:dateTimeStamp
        ];
        fpo:sourcedFrom "Wikipédia"
    .

    _:ch00 a nap:LandmarkChange;
        nap:isChangeType ctype:LandmarkAppearance;
        rdfs:label "Création du département de Seine-et-Oise";
        nap:appliedTo landmark:DEPT_Seine_et_Oise;
        nap:dependsOn _:ev0.

    _:ch01 a nap:LandmarkChange;
        nap:isChangeType ctype:LandmarkDisappearance;
        rdfs:label "Disparition du département de Seine-et-Oise";
        nap:appliedTo landmark:DEPT_Seine_et_Oise;
        nap:dependsOn _:ev1;
        nap:after _:ch00.



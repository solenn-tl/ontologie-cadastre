# ---- Imported properties for annotation ----
skos:inScheme rdf:type owl:AnnotationProperty.
skos:prefLabel rdf:type owl:AnnotationProperty .
skos:altLabel rdf:type owl:AnnotationProperty.
skos:broader rdf:type owl:AnnotationProperty.
skos:definition rdf:type owl:AnnotationProperty.
  
###############################
# ---- Classes definition ----
###############################
nap:Owner a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "Owner"@en , "Propriétaire ou usufruitier"@fr ;
  rdfs:comment "Propriétaire ou usufruitier : personne redevable de l'impôt foncier pour les terres dont elle a la pleine propriété ou l'usufruit."@fr;
  rdfs:subClassOf nap:ParcelAttribute.

## ---- Entités géographiques ----
nap:GeographicalFeature a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "Geographic feature"@en , "Entité géographique"@fr ;
  rdfs:comment "Entité géographique, cadastrale ou administrative."@fr .

nap:Parcel a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "Parcel"@en ,"Parcelle"@fr ;
  rdfs:comment "Parcelle caractérisée par un identifiant, une nature et un ou plusieurs propriétaires."@fr ;
  rdfs:subClassOf nap:GeographicalFeature.

nap:NonParcel a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "Geographical feature which is not a parcel"@en ,"Objet géographique qui n'est pas une parcelle"@fr ;
  rdfs:comment "Espace non cadastré : matérialisé sur les plans mais non soumis à évaluation."@fr ;
  rdfs:subClassOf nap:GeographicalFeature.

nap:Locality a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "Locality"@en ,"Lieu-dit"@fr ;
  rdfs:comment "Précision de la localisation de la parcelle dans la section : généralement un lieu-dit ou un nom de rue."@fr ;
  rdfs:subClassOf nap:GeographicalFeature.

nap:Section a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "Section"@en ,"Section"@fr ;
  rdfs:comment "Section cadastrale."@fr ;
  rdfs:subClassOf nap:GeographicalFeature.

nap:AdministrativeUnit a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "Administrative unit"@en ,"Circonscription administrative"@fr ;
  rdfs:comment "Circonscription administrative."@fr;
  rdfs:subClassOf nap:GeographicalFeature.

nap:Commune a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "Municipality"@en ,"Commune"@fr ;
  rdfs:comment "Plus petit subdivision administrative du territoire français."@fr;
  rdfs:subClassOf nap:AdministrativeUnit.

nap:Canton a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "Canton"@en ,"Canton"@fr ;
  skos:altLabel "Justice de paix"@fr;
  rdfs:comment "Canton dans sa définition du XIXème siècle. Subdivision administrative comprise entre la commune et l'arrondissement."@fr;
  rdfs:subClassOf nap:AdministrativeUnit.

nap:Arrondissement a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "Arrondissement"@en ,"Arrondissement"@fr ;
  rdfs:comment "Subdivision administrative comprise entre le canton et le département."@fr;
  rdfs:subClassOf nap:AdministrativeUnit.

nap:Departement a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "Departement"@en ,"Département."@fr ;
  rdfs:comment "Subdivision administrative chargée de produire et d'administrer le cadastre."@fr;
  rdfs:subClassOf nap:AdministrativeUnit.

## ---- Propriétés d'une entité géographique ----
nap:GeographicalFeatureAttribute a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "type of parcel"@en ,"type de parcelle"@fr ;
  rdfs:comment "Regroupe l'ensemble des attributs utilisés pour décrire une parcelles."@fr.

## ---- Propriétés d'une parcelle ----
nap:ParcelAttribute a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "parcel attribut"@en ,"attribut d'une parcelle"@fr ;
  rdfs:comment "Attribut utilisé pour décrire une parcelle."@fr;
  rdfs:subClassOf nap:GeographicalFeatureAttribute.

nap:ParcelNumber a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "parcel number"@en ,"numéro de parcelle"@fr ;
  rdfs:comment "Numéro attribué à une parcelle."@fr;
  rdfs:subClassOf nap:ParcelAttribute.

nap:ParcelType a owl:Class;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:label "parcel type"@en ,"nature de parcelle"@fr ;
  rdfs:comment "Nature d'une parcelle."@fr;
  rdfs:subClassOf nap:ParcelAttribute.

#############################
# ---- Object properties ----
#############################

nap:hasParcelType a owl:ObjectProperty;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:domain nap:Parcel;
	rdfs:range nap:ParcelType;
	rdfs:label "type of parcel"@en ,"type"@fr ;
	rdfs:comment "Propriété qui relie une parcelle à une nature de parcelle."@fr ,
                 "A property that links a parcel to its type."@en .

nap:isOwnedBy a owl:ObjectProperty;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:domain nap:Parcel;
	rdfs:range nap:Owner;
	rdfs:label "is owned by"@en ,"est détenue par"@fr ;
	rdfs:comment "Propriété qui relie une parcelle à un proprietaire ou usufruitier."@fr ,
                 "A property that links a parcel to its owner."@en .

nap:spatialRelation a owl:ObjectProperty;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:domain nap:GeographicFeature;
	rdfs:range nap:GeographicFeature;
	rdfs:label "spatial relation"@en ,"relation spatiale"@fr ;
	rdfs:comment "Relation spatiale entre deux entités géographiques."@fr ,
                 "Spatial relation between two geographical features."@en .

nap:within a owl:ObjectProperty ;
  rdfs:isDefinedBy <http://data.ign.fr/def/cadastrenap#>;
  rdfs:domain nap:GeographicFeature ;
  rdfs:range nap:GeographicFeature ;
  rdfs:label "fait partie de"@fr,"within"@en;
  rdfs:comment "Relation spatiale qui indique qu'une entité géographique est comprise dans une autre."@fr.

# ---- Imported properties ----
gsp:hasGeometry a rdf:Property;
  rdfs:label "has geometry"@en ;
  rdfs:domain gsp:Feature;
  rdfs:range gsp:Geometry.

###############################
# ---- Datatype properties ----
###############################

# ---- Imported properties ----
rdfs:label a owl:DatatypeProperty.
dcterms:identifier a owl:DatatypeProperty.

###############################
# ---- skos:ConceptScheme ----
###############################

nap:ParcelTypeList a owl:NamedIndividual, skos:ConceptScheme;
	rdfs:comment "Liste des différents types de parcelles, représentés comme une hiérarchie de concepts SKOS."@fr, 
  "List of different types of parcels, represented as a hierarchy of SKOS concepts."@en;
	rdfs:label "Type de parcelles"@fr;
	rdfs:label "Type of parcels"@en.

###################################
# ---- skos:Concept definition ----
###################################

# ------ Concepts of the Scheme parcel:liste -------
# ------ direct subclasses of nap:TypeOfParcel -----

nap:ProprieteNonBatie a owl:NamedIndividual, 
                        nap:ParcelType, 
                        skos:Concept;
	skos:definition "Propriété non bâtie."@fr;
	skos:inScheme nap:ParcelTypeList;
	skos:prefLabel "propriété non bâtie"@fr;
  skos:boader nap:ParcelType.

nap:BareGround a skos:Concept;
	skos:definition "Terre"@fr, "Bare ground"@en;
	skos:inScheme nap:ParcelTypeList;
	skos:prefLabel "terre"@fr, "bare ground"@en;
  skos:boader nap:ProprieteNonBatie.

nap:Vine a skos:Concept;
	skos:definition "Vigne"@fr, "Vine"@en;
	skos:inScheme nap:ParcelTypeList;
	skos:prefLabel "vigne"@fr, "vine"@en;
  skos:boader nap:ProprieteNonBatie.

nap:VineGround a skos:Concept;
	skos:definition "Terre à vigne"@fr;
	skos:inScheme nap:ParcelTypeList;
	skos:prefLabel "terre à vigne"@fr;
  skos:boader nap:ProprieteNonBatie.

nap:ProprieteBatie a owl:NamedIndividual, 
                        nap:ParcelType, 
                        skos:Concept;
	skos:definition "Propriété bâtie."@fr;
	skos:inScheme nap:ParcelTypeList;
	skos:prefLabel "propriété bâtie"@fr;
  skos:boader nap:ParcelType.

nap:House a skos:Concept;
	skos:definition "Maison"@fr, "House"@fr;
	skos:inScheme nap:ParcelTypeList;
	skos:prefLabel "maison"@fr, "house"@en;
  skos:boader nap:ProprietiBatie.